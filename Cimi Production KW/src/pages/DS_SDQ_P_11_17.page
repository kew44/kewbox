<apex:page standardController="DS_SDQ_PSI_CDI_B__c" extensions="DS_SDQ_P_11_17_Controller">

    <c:cimiPageHeader skipElement="{!JSENCODE(skipElementToJson)}" formElement="{!form}"/>
    
    <script src="{!URLFOR($Resource.JS_DS_SDQ)}"></script>
    
    <div class="{!HTMLENCODE(form.visitType)}">
        <apex:sectionHeader title="{!HTMLENCODE(form.title)}" subtitle="{!HTMLENCODE(form.subtitle)}" />
    </div>    
    
    <!--difficult module-->
    <script>
    $j(document).ready(function(){        
        difficultModule({!difficultModuleStatus});        
    });    
    </script>
    <!--end difficult module-->
     
    <div class="bootwrap">
        <!--Begin form content-->
        <apex:form id="DS_SDQ_P_11_17" styleClass="form-horizontal">   
            
            <!-- pageblock to show ClientInformation header as described in Utility class-->
            <c:cimiClientInfo displayVisitType="true" formElement="{!form}"/>
            <!--end client info pageBlock-->
                        
            <!-- primary content pageBlock container-->
            <apex:pageBlock title="{!HTMLENCODE(form.title)}" >
            
                <!--save and cancel control buttons for pageBlock-->
                <apex:pageBlockButtons >
                    <apex:commandButton id="SaveView" action="{!saveview}" value="Save & View Report" styleClass="btn-info btn btn-sm"/>
                    <apex:commandButton id="Save" action="{!save}" value="Save" styleClass="btn-info btn btn-sm"/>
                    <apex:commandButton id="Cancel" action="{!cancel}" value="Cancel" rendered="{!if(form.forceRedirect = '1', false, true)}" styleClass="btn-info btn btn-sm"/>
                </apex:pageBlockButtons><!--end buttons-->
                
                <!--placeholding structure that allows for system message display-->
                <apex:pageMessages />
                                 
                <!--administration date indication-->
                <div class="form-group">
                    <!--question-->
                    <label class="control-label col-md-6 col-lg-6">
                        Date of administration
                    </label>
                    <!--answer -->
                    <div class="col-md-4 col-lg-4 input-date">
                        <apex:inputField id="DOASDOP__c" styleClass="form-control input-date input-sm"  value="{!form.obj[bfVariableMap['DOASDOP']]}"  required="true" showDatePicker="true" html-placeholder="MM/DD/YYYY" />
                    </div>
                </div>
                <!--end administration date indication--> 

                
                <!--administration method indication-->
                <div class="form-group">
                    <!--question-->
                    <label class="control-label col-md-6 col-lg-6">
                        How was this form administered?
                    </label>
                    <!--answer choices-->
                    <div class="col-lg-2 col-md-2">
                        <apex:selectList id="METSDOP__c" styleClass="form-control auto-width" value="{!form.obj[bfVariableMap['METSDOP']]}" size="1" required="true">
                            <apex:selectOptions value="{!BMETOption }"/> 
                        </apex:selectList>
                    </div>
                </div>
                <!--end administration method indication-->   
                
                <!--information/instructions-->
              <apex:outputPanel rendered="{!form.isBaseline}" layout="none">
                    <div class="alert alert-info baseline-alert">
                       <strong>For each item, please mark the box for Not True, Somewhat True, or Certainly True. It would help us if you answered all items as best you can even if you are not absolutely certain. Please give your answers on the basis of your child's behavior over the last six months.</strong>
                    </div> 
                </apex:outputPanel>

                <apex:outputPanel rendered="{!form.isFollowUp}" layout="none">
                    <div class="alert alert-info followup-alert">
                        <strong>For each item, please mark the box for Not True, Somewhat True, or Certainly True. It would help us if you answered all items as best you can even if you are not absolutely certain. Please give your answers on the basis of your child's behavior over the last month.</strong>
                   </div>        
                </apex:outputPanel>
                <!--end info-->
                
