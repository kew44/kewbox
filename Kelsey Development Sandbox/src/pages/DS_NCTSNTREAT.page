<apex:page standardController="DS_Background_Family_Info_F__c" extensions="DS_NCTSNTREAT_Controller">

    <link href="{!URLFOR($Resource.CSS_DS_NCTSNTREAT)}" rel="stylesheet" media="screen, print"/>
       
    <c:cimiPageHeader skipElement="{!JSENCODE(skipElementToJson)}" formElement="{!form}"/>
    
    <div class="{!HTMLENCODE(form.visitType)}">
        <apex:sectionHeader title="{!HTMLENCODE(form.title)}" subtitle="{!HTMLENCODE(form.subtitle)}"/>
    </div> 
    
    <script src="{!URLFOR($Resource.JS_DS_NCTSNTREAT)}"/>
    
    <div class="bootwrap">

        <!---BEGIN NCTSNTREAT FORM Content-->
        <apex:form id="DS_FORM" styleClass="form-horizontal form-nctsntreat">
    
            <!-- pageblock to show ClientInformation header as described in Utility class-->
            <c:cimiClientInfo displayVisitType="true" formElement="{!form}"/>
            <!--end header-->
            
            <!-- NCTSNTREAT information pageBlock container-->
            <apex:pageBlock title="{!HTMLENCODE(form.title)}" >
                
                <!--save and cancel control buttons for pageBlock-->
                <apex:pageBlockButtons >
                    <apex:commandButton action="{!save}"  value="Save" styleClass="btn btn-info btn-sm"/>
                    <apex:commandButton action="{!cancel}"  value="Cancel"  styleClass="btn btn-info btn-sm" rendered="{!if(form.forceRedirect = '1', false, true)}"/>
                </apex:pageBlockButtons><!--end buttons-->
                
                <!--placeholding structure that allows for system message display-->
                <apex:pageMessages />

                <div class="alert alert-info">
                    <strong>In the period since the last assessment, please complete the following about services and treatment that your agency has provided for this child.</strong>
                </div>
                
                <!--1. Received any inpatient -->
                <div class="form-group">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr">1.</span>
                        Has the child received any inpatient or residential treatment?  
                    </label>
                    <div class="col-md-4 col-lg-2">
                        <apex:selectList size="1" styleClass="form-control input-sm " id="TXRES__c" value="{!form.obj[bfVariableMap['TXRES']]}" onChange="skipLogic('TXRES',this.value)">
                            <apex:selectOptions value="{!NYOptionWithBlank}"/>
                        </apex:selectList> 
                    </div>
                </div>     
                
                <div class="form-group skip-TXHOS">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr"></span>
                        Specify type. <strong>(Select all that apply.)</strong> 
                    </label>
                    <div class="col-md-6 col-lg-6">
                        
                        <!--Hospital-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXHOS" id="TXHOS__c" value="{!form.obj[bfVariableMap['TXHOS']]}" label="Public" onChange="skipLogic('TXHOS',this.checked)"/>
                                    Hospital
                                </label> 
                            </div>
                            
                            <div class="skip-TXHOSDAY col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of days: &nbsp;
                                <apex:inputField styleClass="form-control skip-element-TXHOSDAY input-sm auto-width control-inline num-of-day-input"  id="TXHOSDAY__c" value="{!form.obj[bfVariableMap['TXHOSDAY']]}"/>
                            </div>
                        </div>
                        
                        <!--Residential treatment center-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXRTC" id="TXRTC__c" value="{!form.obj[bfVariableMap['TXRTC']]}" label="Public" onChange="skipLogic('TXRTC',this.checked)"/>
                                    Residential treatment center 
                                </label> 
                            </div>
                            <div class="skip-TXRTCDAY col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of days: &nbsp;
                                <apex:inputField styleClass="form-control skip-element-TXRTCDAY input-sm auto-width control-inline num-of-day-input"  id="TXRTCDAY__c" value="{!form.obj[bfVariableMap['TXRTCDAY']]}"/>
                            </div>
                        </div>
                        
                        <!--Group home-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXGH" id="TXGH__c" value="{!form.obj[bfVariableMap['TXGH']]}" label="Public" onChange="skipLogic('TXGH',this.checked)"/>
                                    Group home or other community-based treatment placement
                                </label> 
                            </div>
                            <div class="skip-TXGHDAY col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of days: &nbsp;
                                <apex:inputField styleClass="form-control skip-element-TXGHDAY input-sm auto-width control-inline num-of-day-input"  id="TXGHDAY__c" value="{!form.obj[bfVariableMap['TXGHDAY']]}"/>
                            </div>
                        </div>
                        
                        <!--Other-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label class="col-md-12 col-lg-12 control-label specify-label">
                                        <apex:inputCheckbox styleClass="skip-element-TXOI" id="TXOI__c" value="{!form.obj[bfVariableMap['TXOI']]}" label="Public" onChange="skipLogic('TXOI',this.checked)"/>
                                        Other, please specify: 
                                        <apex:inputField styleClass="form-control input-sm auto-width control-inline skip-element-TXOITYP"  id="TXOITYP__c" value="{!form.obj[bfVariableMap['TXOITYP']]}"/>
                                </label>
                            </div>  
                            <div class="col-md-6 col-lg-6 skip-TXOIDAY num-of-day-div">
                                Please specify number of days: &nbsp;
                               <apex:inputField styleClass="form-control skip-element-TXOIDAY input-sm auto-width control-inline num-of-day-input"  id="TXOIDAY__c" value="{!form.obj[bfVariableMap['TXOIDAY']]}"/>
                            </div>
                        </div>
                        
                    </div>
                    
                </div>    
                
                <!--2. Received any outpatient therapy -->
                <div class="form-group">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr">2.</span>
                        Has the child received any outpatient therapy?   
                    </label>
                    <div class="col-md-4 col-lg-2">
                        <apex:selectList size="1" styleClass="form-control input-sm" id="TXOP__c" value="{!form.obj[bfVariableMap['TXOP']]}" onChange="skipLogic('TXOP',this.value)">
                            <apex:selectOptions value="{!NYOptionWithBlank}"/>
                        </apex:selectList> 
                    </div>
                </div>     
                
                <div class="form-group skip-TXITC">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr"></span>
                        Specify type. <strong>(Select all that apply.)</strong> 
                    </label>
                    <div class="col-md-6 col-lg-6">
                        <!--Individual therapy for child-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXITC" id="TXITC__c" value="{!form.obj[bfVariableMap['TXITC']]}" label="Public" onChange="skipLogic('TXITC',this.checked)"/>
                                    Individual therapy for child 
                                </label> 
                            </div>  
                            <div class="skip-TXITCVIS col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of visits: &nbsp;
                                    <apex:inputField styleClass="form-control skip-element-TXITCVIS input-sm auto-width control-inline num-of-day-input"  id="TXITCVIS__c" value="{!form.obj[bfVariableMap['TXITCVIS']]}"/>
                            </div>
                        </div>
                        
                        <!--Individual therapy for parent-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXITP" id="TXITP__c" value="{!form.obj[bfVariableMap['TXITP']]}" label="Public" onChange="skipLogic('TXITP',this.checked)"/>
                                    Individual therapy for parent     
                                </label> 
                            </div>  
                            <div class="skip-TXITPVIS col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of visits: &nbsp;
                                    <apex:inputField styleClass="form-control skip-element-TXITPVIS input-sm auto-width control-inline num-of-day-input"  id="TXITPVIS__c" value="{!form.obj[bfVariableMap['TXITPVIS']]}"/>
                            </div>
                        </div>
                        
                        <!--Family or Dyadic therapy-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXFDT" id="TXFDT__c" value="{!form.obj[bfVariableMap['TXFDT']]}" label="Public" onChange="skipLogic('TXFDT',this.checked)"/>
                                    Family or Dyadic therapy 
                                </label> 
                            </div>  
                            <div class="skip-TXFDTVIS col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of visits: &nbsp;
                                    <apex:inputField styleClass="form-control skip-element-TXFDTVIS input-sm auto-width control-inline num-of-day-input"  id="TXFDTVIS__c" value="{!form.obj[bfVariableMap['TXFDTVIS']]}"/>
                            </div>
                        </div>
                        
                        <!--Group therapy with other youth -->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXGT" id="TXGT__c" value="{!form.obj[bfVariableMap['TXGT']]}" label="Public" onChange="skipLogic('TXGT',this.checked)"/>
                                    Group therapy with other youth 
                                </label> 
                            </div>  
                            <div class="skip-TXGTVIS col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of visits: &nbsp;
                                    <apex:inputField styleClass="form-control skip-element-TXGTVIS input-sm auto-width control-inline num-of-day-input"  id="TXGTVIS__c" value="{!form.obj[bfVariableMap['TXGTVIS']]}"/>
                            </div>
                        </div>
                        
                        <!--Multi-family group therapy -->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label>
                                    <apex:inputCheckbox styleClass="skip-element-TXMFG" id="TXMFG__c" value="{!form.obj[bfVariableMap['TXMFG']]}" label="Public" onChange="skipLogic('TXMFG',this.checked)"/>
                                    Multi-family group therapy  
                                </label> 
                            </div>  
                            <div class="skip-TXMFGVIS col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of visits: &nbsp;
                                    <apex:inputField styleClass="form-control skip-element-TXMFGVIS input-sm auto-width control-inline num-of-day-input"  id="TXMFGVIS__c" value="{!form.obj[bfVariableMap['TXMFGVIS']]}"/>
                            </div>
                        </div>
                        
                        <!--Other-->
                        <div class="row">
                            <div class="checkbox col-md-6 col-lg-6">
                                <label class="control-label specify-label">
                                        <apex:inputCheckbox styleClass="skip-element-TXOT" id="TXOT__c" value="{!form.obj[bfVariableMap['TXOT']]}" label="Public" onChange="skipLogic('TXOT',this.checked)"/>
                                        Other, please specify:  
                                        <apex:inputField styleClass="form-control input-sm auto-width control-inline skip-element-TXOTTYP"  id="TXOTTYP__c" value="{!form.obj[bfVariableMap['TXOTTYP']]}"/>
                                </label>
                            </div>  
                            <div class="no-border-bottom no-space-bottom skip-TXOTVIS col-md-6 col-lg-6 num-of-day-div">
                                Please specify number of visits: &nbsp;
                                <apex:inputField styleClass="form-control skip-element-TXOTVIS input-sm auto-width control-inline num-of-day-input"  id="TXOTVIS__c" value="{!form.obj[bfVariableMap['TXOTVIS']]}"/>
                            </div>
                        </div>
                        
                    </div>
                    
                </div>   
                
                <!--3. Agency provided services  -->
                <div class="form-group">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr">3.</span>
                        In what setting(s) has your agency provided services for this child and/or family? <strong>(Select all that apply.)</strong>
                    </label>
                    <div class="col-md-6 col-lg-6">
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXCLINIC__c" value="{!form.obj[bfVariableMap['TXCLINIC']]}"/>
                                Clinic 
                            </label> 
                        </div>   
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXHOME__c" value="{!form.obj[bfVariableMap['TXHOME']]}"/>
                                Home 
                            </label> 
                        </div> 
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXSCHOOL__c" value="{!form.obj[bfVariableMap['TXSCHOOL']]}"/>
                                School 
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXDAYPAR__c" value="{!form.obj[bfVariableMap['TXDAYPAR']]}"/>
                                Day treatment/partial hospitalization   
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label class="pull-left">
                                <apex:inputCheckbox id="TXSOTH__c" value="{!form.obj[bfVariableMap['TXSOTH']]}" onChange="skipLogic('TXSOTH',this.checked)"/>
                                Other 
                            </label> 
                            <div class="skip-TXSOTHSP checkbox-other-block">
                                <apex:outputLabel value=", please specify:" for="TXSOTHSP__c"/>
                                <apex:inputField styleClass="form-control input-sm auto-width skip-element-TXSOTHSP" id="TXSOTHSP__c" value="{!form.obj[bfVariableMap['TXSOTHSP']]}"/> 
                            </div>
                        </div>
                        
                    </div>
                </div>   
                
                <!--4. General modalities   -->
                <div class="form-group">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr">4.</span>
                        Please indicate <em>all general modalities</em> of treatment provided. <strong>(Select all that apply.)</strong>
                    </label>
                    <div class="col-md-6 col-lg-6">
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXATTACH__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXATTACH']]}" html-data-smart-select="1"/>
                                Attachment-based therapy   
                            </label> 
                        </div>   
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXBEHATX__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXBEHATX']]}" html-data-smart-select="2"/>
                                Behavioral therapy   
                            </label> 
                        </div> 
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXCOGTX__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXCOGTX']]}" html-data-smart-select="3"/>
                                Cognitive therapy   
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXCOGBEH__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXCOGBEH']]}" html-data-smart-select="4"/>
                                Cognitive behavioral therapy     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXDAYTX__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXDAYTX']]}" html-data-smart-select="5"/>
                                Day treatment or partial hospitalization      
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXEXTX__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXEXTX']]}" html-data-smart-select="6"/>
                                Expressive therapies (Drawing, movement, theater)     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXFAMTX__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXFAMTX']]}" html-data-smart-select="7"/>
                                Family therapy    
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXIHS__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXIHS']]}" html-data-smart-select="8"/>
                                Intensive in-home services    
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXNARTX__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXNARTX']]}" html-data-smart-select="9"/>
                                Narrative therapy     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXPARTR__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXPARTR']]}" html-data-smart-select="10"/>
                                Parent training     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXTPEER__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXTPEER']]}" html-data-smart-select="11"/>
                                Peer therapy    
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXPHARM__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXPHARM']]}" html-data-smart-select="12"/>
                                Pharmacotherapy/medication     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXPOTT__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXPOTT']]}" html-data-smart-select="13"/>
                                Phase-oriented trauma treatment     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXPLAY__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXPLAY']]}" html-data-smart-select="14"/>
                                Play therapy     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXPSYANL__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXPSYANL']]}" html-data-smart-select="15"/>
                                Psychoanalysis     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXPDPT__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXPDPT']]}" html-data-smart-select="16"/>
                                Psychodynamic psychotherapy     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXSCHBAS__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXSCHBAS']]}" html-data-smart-select="17"/>
                                School-based treatment    
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXSST__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXSST']]}" html-data-smart-select="18"/>
                                Social skills training    
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXSOLFC__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXSOLFC']]}" html-data-smart-select="19"/>
                                Solution-focused therapy     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXSTRESS__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXSTRESS']]}" html-data-smart-select="20"/>
                                Stress management/relaxation training     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXSUPTX__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXSUPTX']]}" html-data-smart-select="21"/>
                                Supportive therapy      
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXWAS__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXWAS']]}" html-data-smart-select="22"/>
                                'Wrap around' services     
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label class="pull-left">
                                <apex:inputCheckbox id="TXOTHMOD__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXOTHMOD']]}" onChange="skipLogic('TXOTHMOD',this.checked)" html-data-smart-select="98"/>
                                Other 
                            </label> 
                            <div class="skip-TXOTMOSP checkbox-other-block">
                                <apex:outputLabel value=", please specify:" for="TXOTMOSP__c"/>
                                <apex:inputField styleClass="form-control input-sm auto-width skip-element-TXOTMOSP" id="TXOTMOSP__c" value="{!form.obj[bfVariableMap['TXOTMOSP']]}"/> 
                            </div>
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXNONE__c" styleClass="smart-select-source-Q5" value="{!form.obj[bfVariableMap['TXNONE']]}" html-data-smart-select="0"/>
                                None/Not Applicable    
                            </label> 
                        </div>
                    </div>
                </div> 

                <!--5. primary “general modality" -->
                <div class="form-group">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr">5.</span>
                        Please indicate the <em>primary</em> "general modality" of treatment provided. <strong>(Select only one.)</strong>     
                    </label>
                    <div class="col-md-6 col-lg-4">
                        <apex:selectList size="1" styleClass="form-control input-sm TXMODPRI__c" id="TXMODPRI__c" value="{!form.obj[bfVariableMap['TXMODPRI']]}" onChange="skipLogic('TXMODPRI',this.value)">
                            <apex:selectOptions value="{!TXMODPRIOption}"/>
                        </apex:selectList> 
                    </div>
                </div>

                <!--6. Specific intervention protocols -->
                <div class="form-group">
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr">6.</span>
                        Please indicate all specific intervention protocols provided, if any.  Items should ONLY be checked if treating clinician has been formally trained in the specific intervention protocol. <strong>(Select all that apply.)</strong>
                    </label>
                    <div class="col-md-6 col-lg-6">
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXSIPNO__c" styleClass="smart-select-source-Q7" value="{!form.obj[bfVariableMap['TXSIPNO']]}" html-data-smart-select="1"/>
                                None   
                            </label> 
                        </div>   
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXAFCBT__c" styleClass="smart-select-source-Q7" value="{!form.obj[bfVariableMap['TXAFCBT']]}" html-data-smart-select="2"/>
                                Alternatives for Families: A Cognitive Behavioral Therapy (AF-CBT)   
                            </label> 
                        </div> 
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXDBTSP__c" styleClass="smart-select-source-Q7" value="{!form.obj[bfVariableMap['TXDBTSP']]}" html-data-smart-select="3"/>
                                Adapted Dialectical Behavior Therapy for Special Populations (DBT-SP)  
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXARC__c" styleClass="smart-select-source-Q7" value="{!form.obj[bfVariableMap['TXARC']]}" html-data-smart-select="4"/>
                                Attachment, Self-Regulation, and Competence (ARC): A Comprehensive Framework for Intervention with Complexly Traumatized Youth
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXCPP__c" styleClass="smart-select-source-Q7" value="{!form.obj[bfVariableMap['TXCPP']]}" html-data-smart-select="5"/>
                                Child-Parent Psychotherapy (CPP)       
                            </label> 
                        </div>
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="TXPSB__c" styleClass="smart-select-source-Q7" value="{!form.obj[bfVariableMap['TXPSB']]}" html-data-smart-select="6"/>
                                Children with Problematic Sexual Behavior Cognitive-Behavioral Treatment Program: School-age Program (PSB-CBT)     
                            </label> 
                        </div>
                        <div class="checkbox">
