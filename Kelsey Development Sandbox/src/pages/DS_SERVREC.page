                                <label class="control-label">
                                    <span class="nmbr">1.</span>
                                    Inpatient psychiatric unit or a hospital for mental health problems
                                </label>
                            </td>
                            
                            <!--past 30 days indicator-->
                            <td >
                                <apex:selectRadio styleClass="radio-list radio-list-inline" title="{!form.obj[bfVariableMap['OSINPSY']]}" id="OSINPSY__c" value="{!form.obj[bfVariableMap['OSINPSY']]}" layout="lineDirection">
                                    <apex:selectOptions value="{!NYUOption}"/>
                                </apex:selectRadio>                            
                            </td><!--end 30 days-->
                            
                            
                            <!--lifetime indicator-->
                            <apex:outputPanel rendered="{!form.isBaseline}" layout="none">
