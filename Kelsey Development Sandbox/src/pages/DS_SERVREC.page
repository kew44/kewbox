                        <tr>
                            <th></th>
                            <th>
                                <apex:outputText value="{!tableHeaderTitle}"></apex:outputText>
                            </th>

                            <apex:outputPanel rendered="{!form.isBaseline}" layout="none">
                            <th>Lifetime</th>
                            <th>As a Result of Trauma</th>
                            </apex:outputPanel>

                        </tr>
                    </thead>
