<apex:page standardController="DS_Functioning_Forms_B__c" extensions="DS_CLINEVAL_Controller" ><!--tabStyle="DS_Functioning_Forms_F__c">-->

    <c:cimiPageHeader skipElement="{!JSENCODE(skipElementToJson)}" formElement="{!form}"/>
    
    <div class="{!HTMLENCODE(form.visitType)}">
        <apex:sectionHeader title="{!HTMLENCODE(form.title)}" subtitle="{!HTMLENCODE(form.subtitle)}" />
    </div>
    
    <script src="{!URLFOR($Resource.JS_DS_CLINEVAL)}"/>
    
    <div class="bootwrap">
        <!--Begin form content-->
        <apex:form id="DS_FORM" styleClass="form-horizontal">

            <!-- show ClientInformation header-->
            <c:cimiClientInfo displayVisitType="true" formElement="{!form}"/>
            <!--end client info-->
            
            <!-- primary content pageBlock container-->
            <apex:pageBlock title="{!HTMLENCODE(form.title)}" >
            
                <!--save and cancel control buttons for pageBlock-->
                <apex:pageBlockButtons >
                    <apex:commandButton id="Save" action="{!save}" value="Save" styleClass="btn-info btn btn-sm submitBtn"/>
                    <apex:commandButton id="Cancel" action="{!cancel}" value="Cancel" rendered="{!if(form.forceRedirect = '1', false, true)}" styleClass="btn-info btn btn-sm"/>
                </apex:pageBlockButtons>
                <!--end buttons-->
                
                <!--placeholding structure that allows for system message display-->
                <apex:pageMessages />
                
                <!--instructions -->
                <div class="alert alert-info">
                    <strong>Based on your clinical evaluation, for questions 1-31 please select each problem/symptom/disorder currently displayed by the child. For question 32, please indicate the <i>primary</i> problem/symptom/disorder currently displayed by the child.  Please note that some of the categories have changed to reflect the DSM-5. </strong>
                </div>        
                <!--end instructions--> 
                
                <!--Clinical Evaluation questionaire -->
                <div>
                    <table class="table table-bordered table-hover table-striped table-condensed Clinevaltable">
                    
                        <thead>
                            <!--information sub-heading-->
                            <tr>
                                <th>Clinical Problems, Symptoms, and Disorders</th>
                                <th class="text-center" colspan="3">Child has/exhibits this problem?<br/>(Select all that apply.)</th>
                            </tr>
                            <!--end information-->
                        </thead>
                        
                        <!--radiobutton indicator table-->
                        <tbody>
                            <tr>
                                <td></td>
                                <td class="text-center" style="width:75px">   No   </td><!--child does not have or exhibit the problem-->
                                <td class="text-center" style="width:75px">Probable</td><!--the child probably has or exhibits this problem-->
                                <td class="text-center" style="width:75px">Definite</td><!--the child definitely has or exhibits this problem-->
                            </tr>
                            <!--1. ADHD-->
                            <tr>
                                <td> 
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">1.</span>
                                        Attention-Deficit/Hyperactivity Disorder (ADHD)<br />
                                        (persistent pattern of inattention and/or hyperactivity-impulsivity that interferes with functioning or development)  
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="1" id="Clineval-1-0" name="Clineval-1" value="0" title="{!form.obj[bfVariableMap['CEADHD']]}"/></td><!--No ADHD-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="1" id="Clineval-1-1" name="Clineval-1" value="1" title="{!form.obj[bfVariableMap['CEADHD']]}"/></td><!--Probably ADHD-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="1" id="Clineval-1-2" name="Clineval-1" value="2" title="{!form.obj[bfVariableMap['CEADHD']]}"/></td><!--Definite ADHD-->
                            </tr>
                            
                            <!--2. Bipolar-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">2.</span>
                                        Bipolar Disorder<br/>
                                        (presence of manic episode(s) and major depressive episode(s)) (e.g., Bipolar I, Bipolar II, or related disorders) 
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="2" id="Clineval-2-0" name="Clineval-2" value="0" title="{!form.obj[bfVariableMap['CEBIPL']]}"/></td><!--No Bipolar-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="2" id="Clineval-2-1" name="Clineval-2" value="1" title="{!form.obj[bfVariableMap['CEBIPL']]}"/></td><!--Probably Bipolar-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="2" id="Clineval-2-2" name="Clineval-2" value="2" title="{!form.obj[bfVariableMap['CEBIPL']]}"/></td><!--Definite Bipolar-->
                            </tr>
                            
                            <!--3. Depressive Symptoms-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">3.</span>
                                        Depressive Symptoms<br/>(e.g., depressed mood, diminished interest or pleasure in activities)
                                    </label> 
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="3" id="Clineval-3-0" name="Clineval-3" value="0" title="{!form.obj[bfVariableMap['CEDPS']]}"/></td><!--No Depressive Symptoms-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="3" id="Clineval-3-1" name="Clineval-3" value="1" title="{!form.obj[bfVariableMap['CEDPS']]}"/></td><!--Probably Depressive Symptoms-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="3" id="Clineval-3-2" name="Clineval-3" value="2" title="{!form.obj[bfVariableMap['CEDPS']]}"/></td><!--Definite Depressive Symptoms-->
                            </tr>
                            
                            <!--4. Depressive Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">4.</span>  
                                        Depressive Disorder<br/>
                                        (Disruptive Mood Dysregulation Disorder, Major Depressive Disorder, or related disorders)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="4" id="Clineval-4-0" name="Clineval-4" value="0" title="{!form.obj[bfVariableMap['CEDEP']]}"/></td><!--No Depressive Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="4" id="Clineval-4-1" name="Clineval-4" value="1" title="{!form.obj[bfVariableMap['CEDEP']]}"/></td><!--Probably Depressive Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="4" id="Clineval-4-2" name="Clineval-4" value="2" title="{!form.obj[bfVariableMap['CEDEP']]}"/></td><!--Definite Depressive Disorder-->
                            </tr>
                          
                            <!--5. Traumatic/Complicated Grief-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">5.</span>  
                                        Traumatic/Complicated Grief<br/>
                                        (response to significant loss; can include sadness, rumination about the loss, insomnia, poor appetite, etc.)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="5" id="Clineval-5-0" name="Clineval-5" value="0" title="{!form.obj[bfVariableMap['CETG']]}"/></td><!--No Traumatic/Complicated Grief-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="5" id="Clineval-5-1" name="Clineval-5" value="1" title="{!form.obj[bfVariableMap['CETG']]}"/></td><!--Probably Traumatic/Complicated Grief-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="5" id="Clineval-5-2" name="Clineval-5" value="2" title="{!form.obj[bfVariableMap['CETG']]}"/></td><!--Definite Traumatic/Complicated Grief-->
                            </tr>
                            
                            <!--6. Generalized Anxiety Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">6.</span>  
                                        Generalized Anxiety Disorder <br />
                                        (excessive anxiety and worry about a number of events or activities, such as work or school performance; difficulty controlling worry)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="6" id="Clineval-6-0" name="Clineval-6" value="0" title="{!form.obj[bfVariableMap['CEGA']]}"/></td><!--No Generalized Anxiety Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="6" id="Clineval-6-1" name="Clineval-6" value="1" title="{!form.obj[bfVariableMap['CEGA']]}"/></td><!--Probably Generalized Anxiety Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="6" id="Clineval-6-2" name="Clineval-6" value="2" title="{!form.obj[bfVariableMap['CEGA']]}"/></td><!--Definite Generalized Anxiety Disorder-->
                            </tr>
                            
                            <!--7. Separation Anxiety-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">7.</span>  
                                        Separation Anxiety Disorder<br />
                                        (developmentally-inappropriate and excessive fear or anxiety concerning separation from those to whom the individual is attached)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="7" id="Clineval-7-0" name="Clineval-7" value="0" title="{!form.obj[bfVariableMap['CESEPDS']]}"/></td><!--No Separation Anxiety-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="7" id="Clineval-7-1" name="Clineval-7" value="1" title="{!form.obj[bfVariableMap['CESEPDS']]}"/></td><!--Probably Separation Anxiety-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="7" id="Clineval-7-2" name="Clineval-7" value="2" title="{!form.obj[bfVariableMap['CESEPDS']]}"/></td><!--Definite Separation Anxiety-->
                            </tr>
                            
                            <!--8. Social Phobia-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">8.</span>  
                                        Social Anxiety Disorder (Social Phobia)<br />
                                        (fear or anxiety about one or more social situations in which the individual is exposed to possible scrutiny by others)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="8" id="Clineval-8-0" name="Clineval-8" value="0" title="{!form.obj[bfVariableMap['CESOCAD']]}"/></td><!--No Social Phobia-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="8" id="Clineval-8-1" name="Clineval-8" value="1" title="{!form.obj[bfVariableMap['CESOCAD']]}"/></td><!--Probably Social Phobia-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="8" id="Clineval-8-2" name="Clineval-8" value="2" title="{!form.obj[bfVariableMap['CESOCAD']]}"/></td><!--Definite Social Phobia-->
                            </tr>
                            
                            <!--9. Panic Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">9.</span>  
                                        Panic Disorder <br />
                                        (recurrent unexpected panic attacks)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="9" id="Clineval-9-0" name="Clineval-9" value="0" title="{!form.obj[bfVariableMap['CEPANDS']]}"/></td><!--No Panic Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="9" id="Clineval-9-1" name="Clineval-9" value="1" title="{!form.obj[bfVariableMap['CEPANDS']]}"/></td><!--Probably Panic Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="9" id="Clineval-9-2" name="Clineval-9" value="2" title="{!form.obj[bfVariableMap['CEPANDS']]}"/></td><!--Definite Panic Disorder-->
                            </tr>
                            
                            <!--10. Specific Phobia-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">10.</span> 
                                        Specific Phobia<br />  
                                        (marked fear or anxiety about a specific object or situation)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="10" id="Clineval-10-0" name="Clineval-10" value="0" title="{!form.obj[bfVariableMap['CEPHODS']]}"/></td><!--No Specific Phobia-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="10" id="Clineval-10-1" name="Clineval-10" value="1" title="{!form.obj[bfVariableMap['CEPHODS']]}"/></td><!--Probably Specific Phobia-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="10" id="Clineval-10-2" name="Clineval-10" value="2" title="{!form.obj[bfVariableMap['CEPHODS']]}"/></td><!--Definite Specific Phobia-->
                            </tr>
                            
                            <!-- answer indication sub-heading-->
                            <tr>
                                <td />
                                <td class="text-center" style="width:100px">   No   </td><!--child does not have or exhibit the problem-->
                                <td class="text-center" style="width:100px">Probable</td><!--the child probably has or exhibits this problem-->
                                <td class="text-center" style="width:100px">Definite</td><!--the child definitely has or exhibits this problem-->
                            </tr>
                            <!--end answers-->
                            
                            <!--11. OCD-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">11.</span> 
                                        Obsessive Compulsive and Related Disorders<br />
                                        (presence of obsessions, compulsions, or both) (e.g., trichotillomania [pulling out own hair], hoarding, body dysmorphic disorder, etc.)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="11" id="Clineval-11-0" name="Clineval-11" value="0" title="{!form.obj[bfVariableMap['CEOCD']]}"/></td><!--No OCD-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="11" id="Clineval-11-1" name="Clineval-11" value="1" title="{!form.obj[bfVariableMap['CEOCD']]}"/></td><!--Probably OCD-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="11" id="Clineval-11-2" name="Clineval-11" value="2" title="{!form.obj[bfVariableMap['CEOCD']]}"/></td><!--Definite OCD-->
                            </tr>
                            
                            <!--12. Acute Stress Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">12.</span> 
                                        Acute Stress Disorder <br />
                                        (exposure to actual or threatened death, serious injury, or sexual violation and presence of associated symptoms for up to one month after trauma exposure) 
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="12" id="Clineval-12-0" name="Clineval-12" value="0" title="{!form.obj[bfVariableMap['CEASD']]}"/></td><!--No Acute Stress Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="12" id="Clineval-12-1" name="Clineval-12" value="1" title="{!form.obj[bfVariableMap['CEASD']]}"/></td><!--Probably Acute Stress Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="12" id="Clineval-12-2" name="Clineval-12" value="2" title="{!form.obj[bfVariableMap['CEASD']]}"/></td><!--Definite Acute Stress Disorder-->
                            </tr>
                            
                            <!--13. PTSD-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">13.</span> 
                                        Post-Traumatic Stress Disorder 
                                        (exposure to actual or threatened death, serious injury, or sexual violence and the presence of associated symptoms for more than one month)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="13" id="Clineval-13-0" name="Clineval-13" value="0" title="{!form.obj[bfVariableMap['CEPTSD']]}"/></td><!--No PTSD-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="13" id="Clineval-13-1" name="Clineval-13" value="1" title="{!form.obj[bfVariableMap['CEPTSD']]}"/></td><!--Probably PTSD-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="13" id="Clineval-13-2" name="Clineval-13" value="2" title="{!form.obj[bfVariableMap['CEPTSD']]}"/></td><!--Definite PTSD-->
                            </tr>
                            
                            <!--14. Adjustment Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">14.</span> 
                                        Adjustment Disorder<br />
                                        (development of emotional or behavioral symptoms in response to an identifiable stressor(s) occurring within 3 months of the onset of the stressor(s))
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="14" id="Clineval-14-0" name="Clineval-14" value="0" title="{!form.obj[bfVariableMap['CEADJ']]}"/></td><!--No Adjustment Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="14" id="Clineval-14-1" name="Clineval-14" value="1" title="{!form.obj[bfVariableMap['CEADJ']]}"/></td><!--Probably Adjustment Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="14" id="Clineval-14-2" name="Clineval-14" value="2" title="{!form.obj[bfVariableMap['CEADJ']]}"/></td><!--Definite Adjustment Disorder-->
                            </tr>
                            
                            <!--15. Attachment Problems-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">15.</span> 
                                        Attachment Problems/Difficulties <br />
                                        (difficulty forming and maintaining trusting relationships with other people)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="15" id="Clineval-15-0" name="Clineval-15" value="0" title="{!form.obj[bfVariableMap['CEATTPR']]}"/></td><!--No Attachment Problems-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="15" id="Clineval-15-1" name="Clineval-15" value="1" title="{!form.obj[bfVariableMap['CEATTPR']]}"/></td><!--Probably Attachment Problems-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="15" id="Clineval-15-2" name="Clineval-15" value="2" title="{!form.obj[bfVariableMap['CEATTPR']]}"/></td><!--Definite Attachment Problems-->
                            </tr>
                            
                            <!--16. Dissociative Symptoms-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">16.</span> 
                                        Dissociative Symptoms <br />
                                        (disruption of and/or discontinuity in the normal integration of consciousness, memory, emotion, behavior, etc.)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="16" id="Clineval-16-0" name="Clineval-16" value="0" title="{!form.obj[bfVariableMap['CEDISYMP']]}"/></td><!--No Dissociative Symptoms-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="16" id="Clineval-16-1" name="Clineval-16" value="1" title="{!form.obj[bfVariableMap['CEDISYMP']]}"/></td><!--Probably Dissociative Symptoms-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="16" id="Clineval-16-2" name="Clineval-16" value="2" title="{!form.obj[bfVariableMap['CEDISYMP']]}"/></td><!--Definite Dissociative Symptoms-->
                            </tr>
                            
                            <!--17. Dissociative Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">17.</span> 
                                        Dissociative Disorder <br />
                                        (e.g., Dissociative Identity Disorder, Dissociative Amnesia, etc.) 
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="17" id="Clineval-17-0" name="Clineval-17" value="0" title="{!form.obj[bfVariableMap['CEDISSO']]}"/></td><!--No Dissociative Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="17" id="Clineval-17-1" name="Clineval-17" value="1" title="{!form.obj[bfVariableMap['CEDISSO']]}"/></td><!--Probably Dissociative Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="17" id="Clineval-17-2" name="Clineval-17" value="2" title="{!form.obj[bfVariableMap['CEDISSO']]}"/></td><!--Definite Dissociative Disorder-->
                            </tr>
                            
                            <!--18. Somatization -->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">18.</span> 
                                        Somatization <br />
                                        (one or more somatic symptoms that are distressing or result in significant disruption in daily life) (e.g., abdominal pain, headache, fatigue, nausea, etc.)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="18" id="Clineval-18-0" name="Clineval-18" value="0" title="{!form.obj[bfVariableMap['CESOMAT']]}"/></td><!--No Somatization -->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="18" id="Clineval-18-1" name="Clineval-18" value="1" title="{!form.obj[bfVariableMap['CESOMAT']]}"/></td><!--Probably Somatization -->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="18" id="Clineval-18-2" name="Clineval-18" value="2" title="{!form.obj[bfVariableMap['CESOMAT']]}"/></td><!--Definite Somatization -->
                            </tr>
                            
                            <!--19. Feeding/Eating Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">19.</span> 
                                        Feeding and Eating Disorder <br />
                                        (persistent disturbance of eating or eating-related behavior that results in the altered consumption or absorption of food and that significantly impairs physical health or psychosocial functioning) (e.g., Pica, Anorexia Nervosa, Binge-Eating Disorder, etc.)
                                    </label>
                                </td>
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="19" id="Clineval-19-0" name="Clineval-19" value="0" title="{!form.obj[bfVariableMap['CEEAT']]}"/></td><!--No Feeding/Eating Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="19" id="Clineval-19-1" name="Clineval-19" value="1" title="{!form.obj[bfVariableMap['CEEAT']]}"/></td><!--Probably Feeding/Eating Disorder-->
                                <td class="text-center"><input type="radio" class="Clineval" data-smart-select="19" id="Clineval-19-2" name="Clineval-19" value="2" title="{!form.obj[bfVariableMap['CEEAT']]}"/></td><!--Definite Feeding/Eating Disorder-->
                            </tr>
                            
                            <!--20. Sleep-Wake Disorder-->
                            <tr>
                                <td>
                                    <label class="control-label col-md-12 col-lg-12">
                                        <span class="nmbr">20.</span>  
                                        Sleep-Wake Disorder <br />
                                        (sleep-wake complaints regarding the quality, timing, and amount of sleep) (e.g., narcolepsy, insomnia, etc.)
                                    </label>
                                </td>
