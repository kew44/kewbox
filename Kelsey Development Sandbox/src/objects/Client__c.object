    <validationRules>
        <fullName>Age_smaller_than_22</fullName>
        <active>true</active>
        <errorConditionFormula>AGE__c &gt; 21</errorConditionFormula>
        <errorDisplayField>AGE__c</errorDisplayField>
        <errorMessage>Client cannot be older than 21</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Age_validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; AGE__c !=  FLOOR((TODAY() - DOB1__c)/365.2425)</errorConditionFormula>
        <errorDisplayField>AGE__c</errorDisplayField>
        <errorMessage>Age is wrong</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_birth_cannot_be_future_1</fullName>
        <active>true</active>
        <errorConditionFormula>DOB1__c  &gt;TODAY()</errorConditionFormula>
        <errorDisplayField>DOB1__c</errorDisplayField>
        <errorMessage>Date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_birth_cannot_be_future_2</fullName>
        <active>true</active>
        <errorConditionFormula>DOB2__c  &gt;TODAY()</errorConditionFormula>
        <errorDisplayField>DOB2__c</errorDisplayField>
        <errorMessage>Date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Double_entry_for_dob</fullName>
        <active>true</active>
        <description>Check double entry for Date of Birth</description>
        <errorConditionFormula>DOB1__c !=  DOB2__c</errorConditionFormula>
        <errorMessage>The two date of birth are different.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Owner_Selection</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( CONTAINS( Owner:User.UserRole.Name , LEFT(CENTER__r.Name,4) ) )
&amp;&amp;  (NOT(CONTAINS($Profile.Name, &apos;Power User&apos;))  )</errorConditionFormula>
        <errorMessage>The Clinician you have entered is not available to receive this Client. Please select a Clinician from within your Center.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_number_for_Initial</fullName>
        <active>true</active>
        <errorConditionFormula>ISNUMBER( INIT__c )</errorConditionFormula>
        <errorDisplayField>INIT__c</errorDisplayField>
        <errorMessage>No number for initial</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STOP_and_Go_to_Followup</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(BENRCDS1__c , &quot;Yes&quot;)</errorConditionFormula>
        <errorDisplayField>BENRCDS1__c</errorDisplayField>
        <errorMessage>Do not proceed with Enrollment. Go to the Follow-up Assessment and create a Follow up Visit record.</errorMessage>
    </validationRules>
    <webLinks>
