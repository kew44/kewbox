body div.bootwrap{
	font-size: 1.1em;
}

/* increase size of default salesforce pageblock section */
.apexp .bPageBlock.apexDefaultPageBlock .pbBody .pbSubheader h3 {
font-size: 1.5em;
font-weight: normal;
}

/* add margin before fields start in subsection */
.apexp .bPageBlock.apexDefaultPageBlock .pbBody .pbSubheader{
margin-bottom: 20px;
}

/* add margin below carrot for expanding header */
.apexp .bPageBlock.apexDefaultPageBlock .pbBody .pbSubheader img{
margin-bottom: 5px;
}

.bPageBlock .pbTitle{
width: 50%;
}

/* increase font size for page section */
body .bRelatedList .pbTitle h3, 
body .bPageBlock .pbTitle h2, 
body .bPageBlock .pbTitle h3, 
body .bSubBlock h3.lbHeader {
font-size: 1.6em;
}

/* remove gray lines between fields */
body .bPageBlock .detailList tr td{
border-color: transparent;
}

/* align buttons at top and bottom of forms on right */
.bPageBlock .pbButton, 
.bPageBlock .pbBottomButtons>table>tbody>tr>td{
text-align: right;
}

/* remove extra space between field groups */
body .bPageBlock .pbBody .data2Col{
padding: 0;
}

/* style for labels in horizontal forms */
div.bootwrap .form-horizontal .control-label{
text-align: left;
/*font-size: 1.2em;*/
line-height: 1.4em;
font-weight: normal;
}

span.nmbr{
	display: block;
	float: left;
	height: 20px;
	width: 20px;
}

/* fixes the input sizing for dropdown menus*/
div.bootwrap select[size].input-sm{
height: 30px;
}

/* sets the input to inherit width */
div.bootwrap .form-control.auto-width{
width: auto;
}

/* for the datepicker, sets the input field to be inline with date link */
div.bootwrap .form-control.control-inline{
display: inline;
}

/* adds space to the left of the date picker */
.dateFormat{
padding-left: 10px;
}

/* fixes checkboxes and labels generated by apex */
div.bootwrap .checkbox input[type="checkbox"]{
/*margin-left: 0;*/
}

div.bootwrap .checkbox label{
line-height: 20px;
}

/*sets the font weight of the radiobutton labels to normal*/
div.bootwrap label {
font-weight:normal;
}

.radio-list label{
	padding-left: 8px;
}

.radio-list.radio-list-inline label{
	padding-right: 8px;
}

div.bootwrap .form-group fieldset{
	padding-top: 7px;
}

.specify-label,
.specify-matrix{
	padding-left: 21px;
}

.specify-matrix{
	padding-top: 0px;
	padding-bottom: 7px;
}

.specify-input.specify-matrix{
	width: 125px;
}

.form-group{
	  border-bottom: 1px solid #eaeaea;
	  padding-bottom: 10px;
}

.form-group.no-border-bottom{
	border-bottom: none;
}

div.bootwrap .form-horizontal .form-group{
	margin-left: 0px;
	margin-right: 0px;
}

div.bootwrap .form-horizontal .control-label.label-bold{
	font-weight: 600;
}

div.bootwrap .form-horizontal .control-label.text-right{
	text-align: right;
}

.no-space-bottom,
div.bootwrap .form-horizontal .form-group.no-space-bottom{
	padding-bottom: 0;
	margin-bottom: 0;
}

.header-info-table{
	float: right;
	width: 50%;
	padding-top: 15px;
}

div.bootwrap .header-info-table .table > thead > tr > th, 
div.bootwrap .header-info-table .table .table > tbody > tr > th, 
div.bootwrap .header-info-table .table > tfoot > tr > th, 
div.bootwrap .header-info-table .table .table > thead > tr > td, 
div.bootwrap .header-info-table .table > tbody > tr > td, 
div.bootwrap .header-info-table .table > tfoot > tr > td{
	padding: 5px;
}

.bPageTitle{
	float: left;
	width: 50%;
}

body .bPageTitle img.pageTitleIcon.userDefinedImage{
	width: 40px;
	height: 40px;
	margin-top: 0;
}

#FollowUp.panel.panel-default{
	margin-top: 5px;
}

h3{
	font-size: 1.4em;
}

@media (min-width: 992px) {
	table.military-table{
		table-layout: fixed;
	}
}

div.bootwrap .table-striped > tbody > tr:nth-child(odd){
	background-color: white;
}

div.bootwrap .radio-inline input[type="radio"]{
	margin-top: 0;
}

div.bootwrap .table-domenv th{
	font-weight: normal;
}

div.bootwrap .table-domenv th:nth-of-type(3){
	width: 30%;
}

div.bootwrap .table-domenv input[type="text"]{
	width: 50px;
}

.table-domenv .radio-list label:first-of-type{
	margin-right: 5px;
}

div.form-pageblock-subheader{
  width: 100%;
  padding: 5px;
  background-color: #d8d8d8;
  margin-bottom: 20px;
}

div.bootwrap .form-pageblock-subheader h3{
	font-size: 1.3em;
  	color: #333;
}

div.bootwrap .table-servrec .form-group label{
	padding-left: 0;
}

div.bootwrap .alert.alert-info{
	line-height: 1.5em;
}

/* PTSD RI */
div.bootwrap .form-ptsd-ca h4{
	font-size: 1em;
	font-weight: bold;
	margin-bottom: .5em;
	display: block;
}

div.bootwrap .form-ptsd-ca h5{
	font-size: 1em;
	margin-top: .5em;
	margin-bottom: .5em;
	display: block;
	font-style: italic;
}

div.bootwrap .form-ptsd-ca h5:nth-of-type(2){
	margin-top: 1em;
}

.form-ptsd-ca span.nmbr{
	width: 40px;
}

div.bootwrap input.input-date{
	float: left;
	width: 170px;
}

div.input-date .requiredBlock{
	height: 28px;
}

/* PTSD report */
div.bootwrap .ptsd-report .ptsd-table { border:none; }
div.bootwrap .ptsd-report .ptsd-angle-noborder { border:none; background-color: #f8f8f8; }
div.bootwrap .ptsd-report .ptsd-table thead tr th { text-align: center; vertical-align: top; border-top: 1px solid #ddd !important; }       
div.bootwrap .ptsd-report .first-row td { text-align: center; }
div.bootwrap .ptsd-report .ptsd-table ul { padding: 0px; }
div.bootwrap .ptsd-report .ptsd-angle-icon { cursor: pointer; cursor: hand; }
div.bootwrap .ptsd-report .ptsd-visit-cell-width { min-width: 75px; }
div.bootwrap .ptsd-report .ptsd-table .first-row td { vertical-align: middle; }
div.bootwrap .ptsd-report .ptsd-detail-hide { display: none; }

/* CBCL young and old */
div.bootwrap .form-cbcl span.nmbr {
  	width: 34px;
}


/* Form table */
div.bootwrap .form-table h4 {
  	font-size: 1em;
  	font-weight: bold;
  	margin-bottom: .5em;
  	display: block;
}



div.bootwrap .form-table .striped-question > .form-group:nth-child(odd) { background-color: white; }

div.bootwrap .form-table .striped-question .missing-answer { background-color: #FFE3F1 !important; }

div.bootwrap .form-table .striped-question .form-group:hover {
    background-color: #d9edf7;
}

div.bootwrap .form-table .striped-question .form-group {
    margin-bottom: 0px;
}      

.citation-footer { text-align: center; }

