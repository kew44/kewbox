                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM1B']]}" id="MFM1B__c" value="{!form.obj[bfVariableMap['MFM1B']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!Branch1Items}"/>
                                                </apex:selectRadio>
                                            </td>                                                   
                                            <!--supplementary branch-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM1B2']]}" id="MFM1B2__c" value="{!form.obj[bfVariableMap['MFM1B2']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!Branch2Items}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--end military branches-->
                                            
                                            <!--status-->
