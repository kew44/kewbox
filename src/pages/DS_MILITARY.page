                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM1S']]}" id="MFM1S__c" value="{!form.obj[bfVariableMap['MFM1S']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!StatusItems}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--end status-->
                                            
                                            <!--wartime?-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM1DP']]}" id="MFM1DP__c" value="{!form.obj[bfVariableMap['MFM1DP']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!WartimeItems}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--end wartime-->
                                            
                                            <!--most recent deployment-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM1RDP']]}" id="MFM1RDP__c" value="{!form.obj[bfVariableMap['MFM1RDP']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!DeployItems}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--end most recent-->
                                            
                                            <!--amount of child's life deployed-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM1MODP']]}" id="MFM1MODP__c" value="{!form.obj[bfVariableMap['MFM1MODP']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!DeployAllItems}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--child lifetime deployment-->
                                        </tr>
                                        <!--end family member 1-->
                                        
                                        <!--family member row 2-->                        
                                        <tr class="family-row-2"> 
                                            <!--Relationship to the Child-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFMR2']]}" id="MFMR2__c" value="{!form.obj[bfVariableMap['MFMR2']]}" layout="pageDirection" onchange="skipLogic('MFMR2',this.value)">
                                                    <apex:selectOptions value="{!RelationItems}"/>
                                                </apex:selectRadio>
                                                <!--conditional if other, specify-->
                                                <div class="skip-MFMR2SP form-group no-border-bottom no-space-bottom">
                                                    <label class="control-label specify-label specify-matrix">Please specify:</label>
                                                </div>
                                                <div class="skip-MFMR2SP form-group no-border-bottom no-space-bottom">
                                                    <div class="specify-input specify-matrix">
                                                        <apex:inputField styleClass="form-control skip-element-MFMR2SP" id="MFMR2SP__c" value="{!form.obj[bfVariableMap['MFMR2SP']]}"/>
                                                    </div>
                                                </div>
                                                <!--end conditional-->
                                            </td>  
                                            <!--end relationship-->
                                            
                                            <!--Service Branch-->
                                            <!--active branch-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM2B']]}" id="MFM2B__c" value="{!form.obj[bfVariableMap['MFM2B']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!Branch1Items}"/>
                                                </apex:selectRadio>
                                            </td>                   
                                            <!--supplementary branch-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM2B2']]}" id="MFM2B2__c" value="{!form.obj[bfVariableMap['MFM2B2']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!Branch2Items}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--end military branches-->
                                            
                                            <!--status-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM2S']]}" id="MFM2S__c" value="{!form.obj[bfVariableMap['MFM2S']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!StatusItems}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--end status-->
                                            
                                            <!--wartime?-->
                                            <td >
