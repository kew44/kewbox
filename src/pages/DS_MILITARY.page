                                            <!--end relationship-->
                                            
                                            <!--Service Branch-->
                                            <!--active branch--> 
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM3B']]}" id="MFM3B__c" value="{!form.obj[bfVariableMap['MFM3B']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!Branch1Items}"/>
                                                </apex:selectRadio>
                                            </td> 
                                            <!--supplementary branch-->
                                            <td >
                                                <apex:selectRadio styleClass="radio-list"   title="{!form.obj[bfVariableMap['MFM3B2']]}" id="MFM3B2__c" value="{!form.obj[bfVariableMap['MFM3B2']]}" layout="pageDirection">
                                                    <apex:selectOptions value="{!Branch2Items}"/>
                                                </apex:selectRadio>
                                            </td> 
