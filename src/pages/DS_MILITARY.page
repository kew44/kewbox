<apex:page StandardController="DS_Background_Family_Info_B__c" extensions="DS_MILITARY_Controller" > 
    
    <c:cimiPageHeader skipElement="{!JSENCODE(skipElementToJson)}" formElement="{!form}"/>
    
    <!--Dynamic family member table-->
    <script src="{!URLFOR($Resource.JS_DS_MILITARY)}"></script>  
  <!--  <script src="{!URLFOR($Resource.Scripts, 'DS_MILITARY.js')}"></script>  -->
    
    <div class="{!HTMLENCODE(form.visitType)}">
        <apex:sectionHeader title="{!HTMLENCODE(form.title)}" subtitle="{!HTMLENCODE(form.subtitle)}" />
    </div>
    
    <div class="bootwrap">
        <!--Begin form content-->
        <apex:form id="DS_FORM" styleClass="form-horizontal">
            
            <!-- pageblock to show ClientInformation header as described in Utility class-->
            <c:cimiClientInfo displayVisitType="true" formElement="{!form}"/>
            <!--end client info pageBlock-->
                        
            <!-- primary content pageBlock container-->
            <apex:pageBlock title="{!HTMLENCODE(form.title)}" >
                <!--save and cancel control buttons for pageBlock-->
                <apex:pageBlockButtons >
                    <apex:commandButton id="Save" action="{!save}" value="Save" styleClass="btn btn-info btn-sm"/>
                    <apex:commandButton id="Cancel" action="{!cancel}" value="Cancel" styleClass="btn btn-info btn-sm" rendered="{!if(form.forceRedirect = '1', false, true)}"/>
                </apex:pageBlockButtons>
                <!--end buttons-->

                <!--placeholding structure that allows for system message display-->
                <apex:pageMessages />
    
                <!--Begin Military Form-->
                <apex:outputPanel rendered="{!form.isBaseline}" layout="none">
                    <div class="alert alert-info baseline-alert">
                        <strong>If the child has served, or has family member(s) who served, in the military as indicated by questions 8 or 9 in the Baseline Visit and Demographic Information form, please complete the following questions. <br/><br/>These questions require detailed information; please include caregiver if available.</strong>
                    </div> 
                </apex:outputPanel>

                <apex:outputPanel rendered="{!form.isFollowUp}" layout="none">
                    <div class="alert alert-info followup-alert">
                        <strong>If the child has served, or has family member(s) who served, in the military, as indicated by question 11 in the Follow-Up Visit and Demographic Information form above, please complete the following questions. <br/><br/>These questions require detailed information; please include caregiver if available.</strong>
                    </div>        
                </apex:outputPanel>
                <!--1. relationship-->        
                <div class="form-group">
                
                    <!--q1 question-->
                        <label class="col-md-6 col-lg-6 control-label ">
                            <span class="nmbr">1.</span>
                            What is the relationship of the service member(s) to the child? <strong>(Select all that apply.)</strong>
                        </label>
                        
                    <!--q1 answer choices-->
                    <!--start checkbox list-->
                    <div class="col-md-6 col-lg-6 checkbox">
                        <!--self--> 
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="SELFMIL__c" value="{!form.obj[bfVariableMap['SELFMIL']]}"/>
                                Self  
                            </label>
                        </div>
                        <!--mother-->
                        <div class="checkbox">
                            <label>
                             <apex:inputCheckbox id="MOMMIL__c" value="{!form.obj[bfVariableMap['MOMMIL']]}"/>                           
                                Mother (Biological, adoptive, step) 
                            </label>
                        </div>
                        <!--father-->
                        <div class="checkbox">
                            <label>
                               <apex:inputCheckbox id="DADMIL__c" value="{!form.obj[bfVariableMap['DADMIL']]}"/>                          
                                Father (Biological, adoptive, step) 
                            </label>
                        </div>
                        <!--brother-->
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="BROMIL__c" value="{!form.obj[bfVariableMap['BROMIL']]}"/>                          
                                Brother  
                            </label>
                        </div>
                        <!--sister-->
                        <div class="checkbox">
                            <label>
                               <apex:inputCheckbox id="SISMIL__c" value="{!form.obj[bfVariableMap['SISMIL']]}"/>                          
                                Sister
                            </label>
                        </div>
                        <!--cousin-->
                        <div class="checkbox">
                            <label>
                             <apex:inputCheckbox id="COUMIL__c" value="{!form.obj[bfVariableMap['COUMIL']]}"/>                         
                                Cousin
                            </label>
                        </div>
                        <!--uncle-->
                        <div class="checkbox">
                            <label>
                              <apex:inputCheckbox id="UNCMIL__c" value="{!form.obj[bfVariableMap['UNCMIL']]}"/>                         
                                Uncle
                            </label>
                        </div>
                        <!--aunt-->
                        <div class="checkbox">
                            <label>
                              <apex:inputCheckbox id="AUNMIL__c" value="{!form.obj[bfVariableMap['AUNMIL']]}"/>                          
                                Aunt
                            </label>
                        </div>
                        <!--grandparent-->
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox id="GRAMIL__c" value="{!form.obj[bfVariableMap['GRAMIL']]}"/>                          
                                Grandparent
                            </label>
                        </div>
                        <!--other-->
                        <div class ="checkbox">
                            <label>
                                <apex:inputCheckbox id="OTHMIL__c" value="{!form.obj[bfVariableMap['OTHMIL']]}" onChange="skipLogic('OTHMIL',this.checked)"/>                          
                                Other
                            </label>
                        </div>
                        <!--conditional if other, specify-->
                        <div class="form-group no-border-bottom no-space-bottom skip-OTHMILSP">
                            <apex:outputLabel styleClass="col-md-4 col-lg-4 control-label specify-label" value="Please specify: " for="OTHMILSP__c"/>
                            <div class="col-md-4 col-lg-4 specify-input">
                                <apex:inputField styleClass="form-control input-sm skip-element-OTHMILSP" id="OTHMILSP__c" value="{!form.obj[bfVariableMap['OTHMILSP']]}"/>
                            </div>
                        </div>
                        <!--end conditional-->
                    </div>
                    <!--end checkbox list-->
                    <!--end q1 answer-->
                </div>
                <!--end q1-->                   

                <!--2. relocation amount-->     
                <div class="form-group">
                    <!--q2 question-->
                    <label class="col-md-6 col-lg-6 control-label ">
                        <span class="nmbr">2.</span>
                        How many times did the family (including the child) move or relocate for military reasons or orders? <strong>(Select only one.)</strong>
                    </label>
                    
                    <!--q2 answer choices-->
                    <div class="col-md-6 col-lg-6">                  
                        <apex:selectRadio styleClass="radio-list"  title="{!form.obj[bfVariableMap['MILMOVE']]}" id="MILMOVE__c" value="{!form.obj[bfVariableMap['MILMOVE']]}" layout="pageDirection" >
                            <apex:selectOptions value="{!MoveItems}" />
                        </apex:selectRadio>
                    </div>    
                    <!--end q2 answers-->
                </div>
                <!--end q2-->
                
                <!--directions-->
                <div class="alert alert-info">
                    <strong>If service member(s) indicated in question 1 is the client, caregiver, or immediate family member (e.g., father, sister), please answer the following questions for each family member. <br> If not, skip to the next section of the {!if(form.borf = 'B', 'Baseline', 'Follow-Up')} Visit, "FAPGAR."</br></strong>
                </div>
                <!--end directions-->
                
                <!--3.number of family members-->
                <div class="form-group">
                    <!--q3 question-->
                    <label class="col-md-6 col-lg-6 control-label ">
                        <span class="nmbr">3.</span>
                        What is the total number of immediate family members who have served in the military since 2001? <strong>(Select only one.)</strong>
                    </label>
                    
                    <!--q3 answer choices-->
                    <div class="col-md-6 col-lg-6">      
                        <apex:selectRadio styleClass="radio-list Q3-answer" title="{!form.obj[bfVariableMap['MILSERV']]}" id="MILSERV" value="{!form.obj[bfVariableMap['MILSERV']]}" layout="pageDirection" onchange="('MILSERV',this.value)">
                            <apex:selectOptions value="{!ServeItems}" />
                        </apex:selectRadio>
                    </div>              
                </div>
                <!--end q3-->

                <div class="family-table-div" >
                    <!--4. family member table, conditional based on answer to #3-->
                    <div class="form-group no-border-bottom">   
                        <!--information-->
                        <label class="col-md-12 col-lg-12 control-label ">
                            <span class="nmbr">4.</span>
                            Please answer the questions below for the client and/or each immediate family member counted in question 3. A checked box indicates "Yes" to each item posed.  If the child is the person who served or is serving in the military, choose "Other" under "Family Member" and specify, "self." 
                        </label>
