<apex:page standardController="DS_Background_Family_Info_B__c" extensions="DS_INSDOMENV_Controller">
    
    <c:cimiPageHeader skipElement="{!JSENCODE(skipElementToJson)}" formElement="{!form}"/>
    
    <apex:sectionHeader title="{!form.title}" subtitle="{!form.subtitle}" />

    <div class="bootwrap">
        <!--Begin form content-->
        <apex:form id="DS_FORM" styleClass="form-horizontal">
            
            <!-- pageblock to show ClientInformation header as described in Utility class-->
            <apex:pageBlock title="Client Information">
                <apex:outputText value="{!form.ClientInfo}" escape="false"/>
            </apex:pageBlock><!--end client info pageBlock-->
            
            <!-- primary content pageBlock container-->
            <apex:pageBlock title="{!form.title}" >
                <!--save and cancel control buttons for pageBlock-->
                <apex:pageBlockButtons >
                    <apex:commandButton id="Save" action="{!save}" value="Save" styleClass="btn-info btn btn-sm"/>
                    <apex:commandButton id="Cancel" action="{!cancel}" value="Cancel" rendered="{!if(form.forceRedirect = '1', false, true)}" styleClass="btn-info btn btn-sm"/>
                </apex:pageBlockButtons><!--end buttons-->
                
                <!--placeholding structure that allows for system message display-->
                <apex:pageMessages />
                
                <!-- 1. Is the child currently covered? -->
                <div class="form-group">
                    <label class="col-md-4 control-label">
                        <span class="nmbr">1.</span>
                        Is the child currently covered by any type of public or private health insurance?
                    </label>
                    <div class="col-md-4">
                        <apex:selectRadio id="CINANY__c" value="{!form.obj[bfVariableMap['CINANY']]}" layout="pageDirection" title="{!form.obj[bfVariableMap['CINANY']]}" onChange="skipLogic('CINANY',this.value)">
                            <apex:selectOptions value="{!NYUOption}"/> 
                        </apex:selectRadio>
                    </div>
                </div>
                
                
                <div class="form-group skip-CINPUB">
                    
                    <label class="col-md-4 control-label">
                        <span class="nmbr"></span>
                        What type of public or private health insurance is currently covering the child? (Select all that apply.)
                    </label>
                    
                    <div class="col-md-4">
                        
                        <!--public insurance-->
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox styleClass="skip-element-CINPUB" id="CINPUB__c" value="{!form.obj[bfVariableMap['CINPUB']]}" label="Public" onChange="skipLogic('CINPUB',this.checked)"/>
                                Public
                            </label> 
                        </div> 
                        
                        <div class="skip-CINCHIP">
                            <div class="skipContent">
                                <!--Chip-->
                                <div class="checkbox">
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINCHIP"  id="CINCHIP__c" value="{!form.obj[bfVariableMap['CINCHIP']]}" label="Children’s Health Insurance Program (CHIP)"/>
                                        Children’s Health Insurance Program (CHIP)
                                    </label>
                                </div>
                                <!--Indian Health Service-->
                                <div class="checkbox">
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINIH"  id="CINIH__c" value="{!form.obj[bfVariableMap['CINIH']]}" label="Indian Health Service"/>
                                        Indian Health Service
                                    </label>
                                </div>
                                <!--Medicaid-->
                                <div class="checkbox">
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINMEDD"  id="CINMEDD__c" value="{!form.obj[bfVariableMap['CINMEDD']]}" label="Medicaid"/>
                                        Medicaid
                                    </label>
                                </div>
                                <!--Medicare-->
                                <div class="checkbox">
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINMEDR"  id="CINMEDR__c" value="{!form.obj[bfVariableMap['CINMEDR']]}" label="Medicare"/>
                                        Medicare
                                    </label>
                                </div>
                                <!--Military-->
                                <div class="checkbox">
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINMHC"  id="CINMHC__c" value="{!form.obj[bfVariableMap['CINMHC']]}" label="Military health care (TRICARE, CHAMPVA)"/>
                                        Military health care (TRICARE, CHAMPVA)
                                    </label>
                                </div> 
                                <!--Unknown-->
                                <div class="checkbox">
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINPBUK"  id="CINPBUK__c" value="{!form.obj[bfVariableMap['CINPBUK']]}" label="Unknown"/>
                                        Unknown
                                    </label>
                                </div>                
                                <!--Other-->
                                <div class="checkbox">
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINOPUB"  id="CINOPUB__c" value="{!form.obj[bfVariableMap['CINOPUB']]}" label="Other public" onChange="skipLogic('CINOPUB',this.checked)"/>
                                        Other public
                                    </label>
                                    <div class="skip-CINOPSP"><!--Conditional if other, specify-->
                                        <apex:outputLabel value="Please Specify: " for="CINOPSP__c"/>                            
                                        <apex:inputField styleClass="form-control skip-element-CINOPSP auto-width-inline input-sm"  id="CINOPSP__c" value="{!form.obj[bfVariableMap['CINOPSP']]}" label="Please Specify"/><br/>
                                    </div><!--end other conditional-->
                                </div>
                            </div><!--end checkboxes-->
                        </div>
                        
                        <!--private insurance-->
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox styleClass="skip-element-CINPRIV"  id="CINPRIV__c" value="{!form.obj[bfVariableMap['CINPRIV']]}" label="Private" onChange="skipLogic('CINPRIV',this.checked)"/>
                                Private
                            </label>
                        </div>
                        <div class="skip-CINHMO">
                            
                            <div class="checkbox skipContent">
                                <!--HMO-->
                                <div class="checkbox">                                                
                                    <label>             
                                        <apex:inputCheckbox styleClass="skip-element-CINHMO"  id="CINHMO__c" value="{!form.obj[bfVariableMap['CINHMO']]}" label="HMO"/>
                                        HMO
                                    </label>
                                </div>
                                <!--PPO-->
                                <div class="checkbox">
                                    <label>              
                                        <apex:inputCheckbox styleClass="skip-element-CINPPO"  id="CINPPO__c" value="{!form.obj[bfVariableMap['CINPPO']]}" label="PPO"/>
                                        PPO
                                    </label>
                                </div>
                                <!--fee for service-->
                                <div class="checkbox">
                                    <label> 
                                        <apex:inputCheckbox styleClass="skip-element-CINFFS"  id="CINFFS__c" value="{!form.obj[bfVariableMap['CINFFS']]}" label="Fee-for-service"/>
                                        Fee-for-service
                                    </label>
                                </div>
                                <!--unknown-->
                                <div class="checkbox">
                                    <label> 
                                        <apex:inputCheckbox styleClass="skip-element-CINPRUK"  id="CINPRUK__c" value="{!form.obj[bfVariableMap['CINPRUK']]}" label="Unknown"/>
                                        Unknown
                                    </label>
                                </div>
                                <!--other-->
                                <div class="checkbox">
                                    <label> 
                                        <apex:inputCheckbox styleClass="skip-element-CINOPRI"  id="CINOPRI__c" value="{!form.obj[bfVariableMap['CINOPRI']]}" label="Other private" onChange="skipLogic('CINOPRI',this.checked)"/>
                                        Other private
                                    </label>
                                    <div class="skip-CINOPRS"><!--Conditional if other, specify-->
                                        <apex:outputLabel value="Please Specify: " for="CINOPRS__c"/>
                                        <apex:inputField styleClass="form-control skip-element-CINOPRS auto-width-inline input-sm"  id="CINOPRS__c" value="{!form.obj[bfVariableMap['CINOPRS']]}" label="Please Specify"/>
                                    </div>
                                    <!--end other conditional-->                           
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                <!-- 2. parent/guardian covered by any type of insurance? -->
                <div class="form-group">
                    <label class="col-md-4 control-label">
                        <span class="nmbr">2.</span>
                        Is the child’s parent/guardian covered by any type of insurance?
                    </label>
                    <div class="col-md-4">
                        <apex:selectRadio id="PINANY__c" value="{!form.obj[bfVariableMap['PINANY']]}" layout="pageDirection" title="{!form.obj[bfVariableMap['PINANY']]}" onChange="skipLogic('PINANY',this.value)">
                            <apex:selectOptions value="{!NYUOption}"/> 
                        </apex:selectRadio>
                    </div>
                </div>
                
                <div class="form-group skip-PINPUB">
                    <label class="col-md-4 control-label">
                        <span class="nmbr"></span>
                        What type of public or private health insurance is currently covering the child? (Select all that apply.)
                    </label>
                    <div class="col-md-4">
                        <!--public insurance-->
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox styleClass="skip-element-PINPUB" id="PINPUB__c" value="{!form.obj[bfVariableMap['PINPUB']]}" label="Public" onChange="skipLogic('PINPUB',this.checked)"/>
                                Public
                            </label>  
                        </div>
                        <!--conditional checkboxes if public insurance-->
                        <div class="skip-PINCHIP">
                            <div class="skipContent">
                                <!--Chip-->
                                <div class="checkbox"> 
                                    <label>
                                        <apex:inputCheckbox id="PINCHIP__c" value="{!form.obj[bfVariableMap['PINCHIP']]}" label="Children’s Health Insurance Program (CHIP)"/>
                                        Children’s Health Insurance Program (CHIP) (Parents under 18 years of age)
                                    </label>
                                </div>
                                <!--Indian Health Service-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox id="PINIH__c" value="{!form.obj[bfVariableMap['PINIH']]}" label="Indian Health Service"/>
                                        Indian Health Service
                                    </label>
                                </div>
                                <!--Medicaid-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox id="PINMEDD__c" value="{!form.obj[bfVariableMap['PINMEDD']]}" label="Medicaid"/>
                                        Medicaid
                                    </label>
                                </div>
                                <!--Medicare-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox id="PINMEDR__c" value="{!form.obj[bfVariableMap['PINMEDR']]}" label="Medicare"/>
                                        Medicare
                                    </label>
                                </div>
                                <!--Military-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox id="PINMHC__c" value="{!form.obj[bfVariableMap['PINMHC']]}" label="Military health care (TRICARE, CHAMPVA)"/>
                                        Military health care (TRICARE, CHAMPVA)
                                    </label>
                                </div>  
                                <!--Other-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox styleClass="skip-element-PINOPUB" id="PINOPUB__c" value="{!form.obj[bfVariableMap['PINOPUB']]}" label="Other Public" onChange="skipLogic('PINOPUB', this.checked)"/>
                                        Other public
                                    </label>
                                    <div class="skip-PINOPSP"><!--Conditional if other, specify-->
                                        <apex:outputLabel value="Please Specify: "  for="PINOPSP__c"/>
                                        <apex:inputField styleClass="form-control skip-element-PINOPSP auto-width-inline input-sm" id="PINOPSP__c" value="{!form.obj[bfVariableMap['PINOPSP']]}" label="Please Specify"/><br/>
                                    </div><!--end other conditional-->
                                </div>
                            </div><!--end checkboxes-->
                        </div>
                        
                        <!--private insurance-->
                        <div class="checkbox">
                            <label>
                                <apex:inputCheckbox styleClass="skip-element-PINPRIV"  id="PINPRIV__c" value="{!form.obj[bfVariableMap['PINPRIV']]}" label="Private" onChange="skipLogic('PINPRIV',this.checked)"/>
                                Private
                            </label>
                        </div>
                        <!--conditional checkboxes if private insurance-->
                        <div class="skip-PINHMO">
                            <div class="skipContent">
                                <!--HMO-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox id="PINHMO__c" value="{!form.obj[bfVariableMap['PINHMO']]}" label="HMO"/>
                                        HMO
                                    </label>
                                </div>
                                <!--PPO-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox id="PINPPO__c" value="{!form.obj[bfVariableMap['PINPPO']]}" label="PPO"/>
                                        PPO
                                    </label>
                                </div>
                                <!--fee for service-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox id="PINFFS__c" value="{!form.obj[bfVariableMap['PINFFS']]}" label="Fee-for-service  "/>
                                        Fee-for-service
                                    </label>
                                </div>
                                <!--other-->
                                <div class="checkbox">
                                    <label>
                                        <apex:inputCheckbox styleClass="skip-element-PINOPRI"  id="PINOPRI__c" value="{!form.obj[bfVariableMap['PINOPRI']]}" label="Other private" onChange="skipLogic('PINOPRI',this.checked)"/>
                                        Other private
                                    </label>
                                    <div class="skip-PINOPRS"><!--Conditional if other, specify-->
                                        <apex:outputLabel value="Please Specify: " for="PINOPRS__c"/>
                                        <apex:inputField styleClass="form-control skip-element-PINOPRS auto-width-inline input-sm"  id="PINOPRS__c" value="{!form.obj[bfVariableMap['PINOPRS']]}" label="Please Specify"/>
                                    </div><!--end other conditional-->     
                                </div>
                            </div>
                        </div>
                        <!--end private conditional-->
                    </div>
                </div>
                
                <!--Completion status control-->
                <div class="form-group">
                    <label class="control-label col-md-2 col-md-offset-8">Complete?</label>
                    <div class="col-md-2">
                        <apex:selectList id="STATUS_INSDOMENV__c" styleClass="pull-right form-control input-sm auto-width-inline" value="{!form.obj[bfVariableMap['STATUS']]}" size="1">
                            <apex:selectOption itemValue="Incomplete" itemLabel="Incomplete"/>
                            <apex:selectOption itemValue="Complete" itemLabel="Complete"/>
                        </apex:selectList> 
                    </div>
                </div>
                <!--end completion status-->
                
            </apex:pageBlock><!--end insdomenv pageBlock-->
            
        </apex:form>
        <!--end form content-->
        
    </div>
    
    <!--citation information footer-->
    <p class="footer" style="text-align:center ">
        <i>This form is part of the Clinical Improvement through Measurement Initiative (CIMI) of the National Child Traumatic Stress Network (NCTSN) - Baseline - CRF Version 6 2014
        </i>
    </p>
    
</apex:page>