<apex:page standardController="DS_Background_Family_Info_B__c" extensions="DS_DEMO_Controller">

    <c:cimiPageHeader skipElement="{!JSENCODE(skipElementToJson)}" formElement="{!form}"/> 
    
    <script src="{!URLFOR($Resource.JS_DS_DEMO)}"></script>     
    
    <!--nodd module, if no defect or disability-->
    <script> $j(document).ready(function(){ noddModule(); }); </script>   

    <div class="{!HTMLENCODE(form.visitType)}">
        <apex:sectionHeader title="{!HTMLENCODE(form.title)}" subtitle="{!HTMLENCODE(form.subtitle)}" />
    </div>
    
    <div class="bootwrap">
        
        <!--Begin form content-->
        <apex:form id="DS_DEMO" styleClass="form-horizontal">
            
            <!-- pageblock to show ClientInformation header as described in Utility class-->
            <c:cimiClientInfo displayVisitType="true" formElement="{!form}"/>
            <!--end client info pageBlock-->     
            
            <!-- primary content pageBlock container-->
            <apex:pageBlock title="{!HTMLENCODE(form.title)}" >
                
                <!--save and cancel control buttons for pageBlock-->
                <apex:pageBlockButtons >
                    <apex:commandButton id="Save" action="{!save}" value="Save" styleClass="btn-info btn btn-sm"/>
                    <apex:commandButton id="Cancel" action="{!cancel}" value="Cancel" rendered="{!if(form.forceRedirect = '1', false, true)}" styleClass="btn-info btn btn-sm"/>
                </apex:pageBlockButtons><!--end buttons-->
                
                <!--placeholding structure that allows for system message display-->
                <apex:pageMessages />
                
                <div class="alert alert-info">
                    <!-- BEGIN BASELINE VISIT INFORMATION -->    
                    Baseline Visit Information
                </div>
                <!-- 1. Date of visit -->       
                <div class="form-group">
                    <!--q1 question-->
                    <label class="control-label col-md-6 col-lg-6">
                        <span class="nmbr">1.</span>
                        Date of visit
                    </label>
                    
                    <!--q1 answer field-->
                    <div class="col-md-4 col-lg-4 input-date">
                        <apex:inputField showDatePicker="true" html-placeholder="MM/DD/YYYY"  styleClass="form-control input-date input-sm" value="{!form.obj[bfVariableMap['DOV']]}" required="true" />
                    </div>
                </div>    
                <!--end q1-->
                <!--2. How was form administered ? -->
                <div class="form-group">
                    <!--q2 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">2.</span>
                        How was this form administered?
                    </label>
                    
                    <!--q2 answer-->
                    <div class="col-lg-2 col-md-2">
                        <apex:selectList id="MET__c" styleClass="form-control input-sm" value="{!form.obj[bfVariableMap['MET']]}" size="1" required="true">
                            <apex:selectOptions value="{!BMETOption}"/> 
                        </apex:selectList>
                    </div>
                </div>
                <!--end q2-->
                <!--3. Visit Type-->
                <div class="form-group">
                    <!--q3 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">3.</span>
                        Is this the child's first visit at this center for the current episode of care?
                    </label>
                    <!--q3 answer choices-->
                    <div class="col-md-4 col-lg-2">
                        <apex:selectList id="CUREPI__c" styleClass="form-control input-sm" value="{!form.obj['CUREPI__c']}" size="1" onChange="skipLogic('CUREPI',this.value)">
                            <apex:selectOptions value="{!NYOptionWithBlank}"/> 
                        </apex:selectList>                         
                        <!-- 3a. Conditional if "no" is selected -->
                        <div class="skip-CUREPVS">
                            <!--label/ instructions-->
                            <label class=" control-label">
                                How many visits (including today's visit) has the child had at your center for the current episode of care?
                            </label>
                            <div><br/>
                                Number of visits:
                                &nbsp;
                                <apex:inputField styleClass="form-control auto-width input-sm skip-element-CUREPVS" value="{!form.obj['CUREPVS__c']}"/>
                            </div>
                        </div>
                        <!--end conditional-->
                    </div>
                </div>
                <!-- 4. From who are you collecting Information? -->
                <div class="form-group">
                    <!--q4 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">4.</span>
                        From whom are you collecting information for this form? <strong>(Select all that apply.)</strong>
                    </label>
                    <!--q4 answer field-->
                    <div class="col-md-4 col-lg-4" >
                        <!-- Parent -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput "  value="{!form.obj[bfVariableMap['INFPAR']]}" id="INFPAR__c"/>
                                Parent (Biological, adoptive, step)
                            </label>
                        </div>                                    
                        <!-- Other adult relative -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput "  value="{!form.obj[bfVariableMap['INFOADL']]}" id="INFOADL__c"/>
                                Other adult relative
                            </label>
                        </div>                                    
                        <!-- Foster parent -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput "  value="{!form.obj[bfVariableMap['INFFOS']]}" id="INFFOS__c"/>
                                Foster parent
                            </label>
                        </div>                                    
                        <!-- Agency staff -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput "  value="{!form.obj[bfVariableMap['INFAGCY']]}" id="INFAGCY__c"/>
                                Agency staff
                            </label>
                        </div>                                    
                        <!-- Child/ adolescent/ self -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput "  value="{!form.obj[bfVariableMap['INFCHLD']]}" id="INFCHLD__c"/>
                                Child/adolescent/self
                            </label>
                        </div>                                    
                        <!-- Collateral sources -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput "  value="{!form.obj[bfVariableMap['INFCOLL']]}" id="INFCOLL__c"/>
                                Collateral sources (e.g., Child welfare, case worker)
                            </label>
                        </div>                                    
                        <!-- Other -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput "  value="{!form.obj[bfVariableMap['INFOTHR']]}" id="INFOTHR__c" onChange="skipLogic('INFOTHR',this.checked)"/>
                                Other
                            </label>                                    
                            <!-- Conditional, shown when "Other is checked" -->
                            <div class="skip-INFOSP">
                                <apex:outputLabel value=" Please specify: " for="INFOSP__c"/>
                                <apex:inputField styleClass="form-control input-sm auto-width skip-element-INFOSP" id="INFOSP__c" value="{!form.obj[bfVariableMap['INFOSP']]}"/>
                            </div><!--end conditional-->
                        </div>
                    </div><!--end checkboxes-->
                </div>
                <!-- 5. Who is the legal guardian -->
                <div class="form-group">
                    <!--q5 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">5.</span>
                        Who is currently the legal guardian for this child? <strong>(Select only one.)</strong>
                    </label>
                    <!--q5 answer field-->
                    <div class="col-md-6 col-lg-6">
                        <apex:selectRadio id="GUARD__c" styleClass="radio-list" layout="pageDirection" value="{!form.obj[bfVariableMap['GUARD']]}" onChange="skipLogic('GUARD',this.value)">
                            <apex:selectOptions value="{!Q4Items}"/> 
                        </apex:selectRadio>
                        <!-- Conditionally shown if guardian is indicated as Other-->
                        <div class="form-group  skip-GUARDSP">
                            <apex:outputLabel value="Please specify: " for="GUARDSP__c"/>
                            <apex:inputField styleClass="form-control input-sm auto-width skip-element-GUARDSP" id="GUARDSP__c" value="{!form.obj[bfVariableMap['GUARDSP']]}"/>
                        </div><!--end conditional-->
                    </div>
                </div>
                <!-- 6. How was child referred? -->
                <div class="form-group">
                    <!--q6 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">6.</span>
                        How was this child referred for services? <strong>(Select all that apply.)</strong>
                    </label>
                    <!--q6 answer field-->
                    <div class="col-md-4 col-lg-4" >
                        <div class="checkbox">                                     
                            <!--Self-Referral-->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFSR']]}" id="REFSR__c"/>
                                    Self-Referral
                                </label>
                            </div>                                    
                            <!--Peer Support-->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFSPR']]}" id="REFSPR__c"/>
                                    Peer Support (e.g., A friend, community support group)
                                </label>
                            </div>                                    
                            <!--Mental Health Provider -->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFMTH']]}" id="REFMTH__c"/>
                                    Mental Health Provider 
                                </label>
                            </div>                                    
                            <!--Courts-->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFSCT']]}" id="REFSCT__c"/>
                                    Courts
                                </label>
                            </div>                                    
                            <!--Child Welfare-->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFSCW']]}" id="REFSCW__c"/>
                                    Child Welfare
                                </label>
                            </div>                                    
                            <!--School-->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFSCOL']]}" id="REFSCOL__c"/>
                                    School
                                </label>
                            </div>                                    
                            <!--Medical Health Provider-->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFMHP']]}" id="REFMHP__c"/>
                                    Medical Health Provider
                                </label>
                            </div>                                    
                            <!--Other-->
                            <div class="checkbox">
                                <label>
                                    <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['REFSRO']]}" id="REFSRO__c" onChange="skipLogic('REFSRO',this.checked)"/>
                                    Other
                                </label>                        
                            </div>                
                            <!-- Conditionally shown when other referral source -->
                            <div class="form-group col-md-6 col-lg-6 skip-REFSOSP">
                                <apex:outputLabel value="Please specify: " for="REFSOSP__c"/>
                                <apex:inputField styleClass="form-control input-sm auto-width skip-element-REFSOSP" id="REFSOSP__c" value="{!form.obj[bfVariableMap['REFSOSP']]}"/>
                            </div><!--end conditional-->
                        </div><!--end checkboxes-->
                    </div>
                </div>
                <!-- 7. Specify zip -->
                <div class="form-group">
                    <!--q7 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">7.</span>
                        Please specify ZIP code of child's current residence:    
                    </label>
                    <!--q7 answer field-->
                    <div class="col-md-4 col-lg-4" >  
                        <apex:inputField styleClass="form-control auto-width input-sm" id="NZCODESP__c" value="{!form.obj[bfVariableMap['NZCODESP']]}"/>&nbsp;(5 digit zip code)
                        <br />
                        <br/>
                        <strong>Or, check the following if unknown: &nbsp;</strong>
                        <apex:inputField styleClass="checkinput" id="ZIPCODECHECK__c" value="{!form.obj[bfVariableMap['ZIPCODECHECK']]}"/>         
                    </div>
                </div>
                <!-- 8. What services do you intend to provide? -->
                <div class="form-group">
                    <!--q8 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">8.</span>
                        What additional services do you intend to provide to the client during the client's course of treatment? <strong>(Select all that apply.)</strong>
                    </label>
                    <!--q8 answer field-->
                    <div class="col-md-4 col-lg-4" >
                        <!--Assessment/screening-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATA']]}" id="TREATA__c"/>
                                Assessment/screening
                            </label>
                        </div>                              
                        <!--Brief intervention-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATBI']]}" id="TREATBI__c"/>
                                Brief intervention 
                            </label>
                        </div>                              
                        <!--Brief treatment services-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATBS']]}" id="TREATBS__c"/>
                                Brief treatment services 
                            </label>
                        </div>                              
                        <!--Evidence-based treatment-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATEBT']]}" id="TREATEBT__c"/>
                                Evidence-based treatment (e.g., TF-CBT/CPP)  
                            </label>
                        </div>                              
                        <!--Ancillary services-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATANS']]}" id="TREATANS__c"/>
                                Ancillary services (e.g., case management) 
                            </label>
                        </div>                              
                        <!--Referral to treatment-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATREF']]}" id="TREATREF__c"/>
                                Referral to treatment services
                            </label>
                        </div>                              
                        <!--Treatment/ Recovery planning-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATRECO']]}" id="TREATRECO__c"/>
                                Treatment/ Recovery planning
                            </label>
                        </div>                              
                        <!--Individual counseling-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATINDV']]}" id="TREATINDV__c"/>
                                Individual Counseling
                            </label>
                        </div>                              
                        <!-- Group counseling-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATGROUP']]}" id="TREATGROUP__c"/>
                                Group Counseling
                            </label>
                        </div>                              
                        <!--Family counseling-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATFAM']]}" id="TREATFAM__c"/>
                                Family Counseling
                            </label>
                        </div>                                          
                        <!-- Unknown-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATU']]}" id="TREATU__c"/>
                                Unknown 
                            </label>
                        </div>                  
                        <!--Other-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput" value="{!form.obj[bfVariableMap['TREATO']]}" id="TREATO__c" onChange="skipLogic('TREATO',this.checked)"/>
                                Other
                            </label>      
                        </div>                              
                        <!-- Conditional if other service intended -->
                        <div class="form-group col-md-6 col-lg-6 skip-TREATOSP">
                            <apex:outputLabel value="Please specify: " for="TREATOSP__c"/>
                            <apex:inputField styleClass="form-control input-sm auto-width skip-element-TREATOSP" id="TREATOSP__c" value="{!form.obj[bfVariableMap['TREATOSP']]}"/>
                        </div><!--end conditional-->
                    </div><!--end checkboxes-->
                </div>                        
                <!-- END BASELINE VISIT INFORMATION -->
                
                <!-- BEGIN DEMOGRAPHIC INFORMATION -->
                <div class="alert alert-info">Demographic Information</div>
                
                <!-- 1. Child's gender -->       
                <div class="form-group">
                    <!--q1 question-->
                    <label class="control-label col-md-6 col-lg-6">
                        <span class="nmbr">1.</span>
                        What is the child's current gender identity (for clients 5 or older)?  
                    </label>
                    
                    <!--q1 answer field-->
                    <div class="col-md-4 col-lg-2">
                        <apex:selectList id="IDENT__c" styleClass="form-control" value="{!form.obj[bfVariableMap['IDENT']]}" size="1" onchange="skipLogic('IDENT',this.value)">
                            <apex:selectOptions value="{!GenIdenItems}"/> 
                        </apex:selectList>   <br/>                           
                        <!-- Conditionally shown if gender identity is other -->
                        <div  class="skip-IDENTSP">
                            <apex:outputLabel value="Please specify:  " for="IDENTSP"/>
                            <apex:inputField styleClass="form-control auto-width skip-element-IDENTSP input-sm" id="IDENTSP__c" value="{!form.obj[bfVariableMap['IDENTSP']]}"/>
                        </div><!--end conditional-->
                    </div>
                </div>                        
                <!-- 2. Child's gender (12 or older) -->
                <div class="form-group">
                    <!--q2 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">2.</span>
                        Which of the following do you consider yourself to be (for clients 12 or older)?
                    </label>
                    <!--q2 answer-->
                    <div class="col-md-4 col-lg-2">
                        <apex:selectList id="ORIENT__c" styleClass="form-control" value="{!form.obj[bfVariableMap['ORIENT']]}" size="1" onchange="skipLogic('ORIENT',this.value)" >
                            <apex:selectOptions value="{!OrientItems}"/> 
                        </apex:selectList> <br/>
                        <!--Conditionally shown if orientation is other-->
                        <div class="skip-ORIENTSP">
                            <apex:outputLabel value="Please specify:  " for="ORIENTSP"/>
                            <apex:inputField styleClass="form-control auto-width skip-element-ORIENTSP input-sm" id="ORIENTSP__c" value="{!form.obj[bfVariableMap['ORIENTSP']]}"/>
                        </div><!--end conditional-->
                    </div>
                </div>                        
                <!-- 3. Child's ethnicity -->
                <div class="form-group">
                    <!--q3 label-->
                    <label class="control-label  col-md-6 col-lg-6">
                        <span class="nmbr">3.</span>Child's ethnicity: <strong>(Select only one.)</strong>
                    </label>
                    <!--q3 answer choices-->
                    <div class="col-md-4 col-lg-2">
                        <apex:selectList id="ETHNIC__c" styleClass="form-control" value="{!form.obj['ETHNIC__c']}" size="1" onChange="skipLogic('ETHNIC',this.value)">
                            <apex:selectOptions value="{!HispLatItems}"/> 
                        </apex:selectList>         
                    </div>
                </div>    
                <!-- Conditional if Hispanic or Latino ethnicity is indicated-->
                <div class="form-group  skip-ETHNICHCA ">
                    <label class= "col-md-6 col-lg-6 control-label">
                        <span class="nmbr" />
                        What ethnic group does the child belong to? <strong>(Select all that apply.)</strong>
                    </label>          
                    <!--Central American-->
                    <div class="col-md-6 col-lg-6">
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHCA__c']}" styleClass="skip-element-ETHNICHCA" id="ETHNICHCA__c"/> 
                                Central American  
                            </label>
                        </div>                                        
                        <!--Cuban -->
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHC__c']}" styleClass="skip-element-ETHNICHC" id="ETHNICHC__c"/>
                                Cuban  
                            </label>
                        </div>                                        
                        <!--Dominican-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHD__c']}" styleClass="skip-element-ETHNICHD" id="ETHNICHD__c"/>
                                Dominican  
                            </label>
                        </div>                                        
                        <!--Mexican-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHM__c']}" styleClass="skip-element-ETHNICHM" id="ETHNICHM__c"/>
                                Mexican  
                            </label>
                        </div>                                        
                        <!--Puerto Rican-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHPR__c']}" styleClass="skip-element-ETHNICHPR" id="ETHNICHPR__c"/>
                                Puerto Rican  
                            </label>
                        </div>                                        
                        <!--South American-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHSA__c']}" styleClass="skip-element-ETHNICHSA" id="ETHNICHSA__c"/>
                                South American  
                            </label>
                        </div>          
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHUK__c']}" styleClass="skip-element-ETHNICHUK" id="ETHNICHUK__c"/>
                                Unknown
                            </label>
                        </div> <!-- end checkboxes -->                              
                        <!--Other-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField value="{!form.obj['ETHNICHO__c']}" styleClass="skip-element-ETHNICHO" id="ETHNICHO__c" onchange="skipLogic('ETHNICHO',this.checked)"/>
                                Other 
                            </label>
                        </div>                    
                        <!-- Conditionally if other Hispanic or Latino ethnicity is indicated-->
                        <div class="col-md-4 col-lg-4 skip-ETHNICHOSP">
                            <apex:outputLabel value=" Please specify: " for="ETHNICHOSP__c"/>
                            <apex:inputField styleClass="form-control input-sm auto-width skip-element-ETHNICHOSP" id="ETHNICHOSP__c" value="{!form.obj['ETHNICHOSP__c']}"/>
                        </div><!--end conditional-->                    
                        <!--Unknown-->
                        <!-- end skip content -->
                    </div> 
                </div>
                <!-- 4. Child's race -->
                <div class="form-group">
                    <!--q4 question-->
                    <label class="col-md-6 col-lg-6 control-label">
                        <span class="nmbr">4.</span>
                        Child's race:<strong> (If multiracial, select all that apply.)</strong>
                    </label>
                    <!--q4 answer choices-->
                    <div class="col-md-4 col-lg-4">
                        <!--White-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RWHITE" value="{!form.obj['RWHITE__c']}" id="RWHITE__c"/>
                                White
                            </label>
                        </div>                                
                        <!--Black-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RBLACK" value="{!form.obj['RBLACK__c']}" id="RBLACK__c"/>
                                Black/ African American
                            </label>
                        </div>                                
                        <!--American Indian-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RINDIAN" value="{!form.obj['RINDIAN__c']}" id="RINDIAN__c"/>
                                American Indian
                            </label>
                        </div>                                
                        <!--Alaska Native-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RAKNTV" value="{!form.obj['RAKNTV__c']}" id="RAKNTV__c" />
                                Alaska Native
                                <br />
                                <br />
                            </label>
                        </div>                                
                        <!--Native Hawaiian-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RHAWAN" value="{!form.obj['RHAWAN__c']}" id="RHAWAN__c"/>
                                Native Hawaiian 
                            </label>
                        </div>                                
                        <!--Guamanian or Chamorro-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RGUAM" value="{!form.obj['RGUAM__c']}" id="RGUAM__c"/>
                                Guamanian or Chamorro
                            </label>
                        </div>                                
                        <!--Samoan-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RSAMOA" value="{!form.obj['RSAMOA__c']}" id="RSAMOA__c"/>
                                Samoan
                            </label>
                        </div>                                
                        <!--Other Pacific Islander-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-ROPI" value="{!form.obj['ROPI__c']}" id="ROPI__c"/>
                                Other Pacific Islander
                                <br />
                                <br />
                            </label>
                        </div>                                
                        <!--Asian Indian-->
                        <div class="checkbox">
                            <label>
                                <apex:inputField styleClass="checkinput skip-element-RASIANIN" value="{!form.obj['RASIANIN__c']}" id="RASIANIN__c"/>
                                Asian Indian
                            </label>
