                                    <!--JQuery script for allowing de-selection of radiobuttons--> 
                                    <script>
                                    $j('.Clineval').click(function(){
                                        
                                        var radioValue = '';
                                        var itemNumber = $j(this).val();
                                        var attrName = $j(this).attr('name');
                                        
                                        setTimeout(function(){
                                            radioValue = ($j('#'+attrName+'-'+itemNumber).is(':checked')) ? itemNumber : '';
                                            $j('.' + attrName).val( radioValue );
                                        }, 0);
                                        
                                    });
                                    
                                    for(var i = 1; i <= 32; i++) {  
                                        if($j('.Clineval-' + i).val() != '') 
                                            $j('#Clineval-' + i + '-' + $j('.Clineval-'+i).val()).prop('checked',true);
                                    }                
                                    </script><!--End JQuery deselect radiobutton value script block-->
                                    
                                </div> 
                            </td>
                        </tr>
                        
                        <!--30. Additional problems?-->
                        <tr>          
