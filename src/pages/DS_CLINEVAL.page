                                    <apex:inputField value="{!obj.BCEPANDS__c}"  styleClass="Clineval-9"  style="display:none;"/><!--Panic Disorder -->
                                    <apex:inputField value="{!obj.BCEPHODS__c}"  styleClass="Clineval-10"  style="display:none;"/><!--Specific Phobia -->
                                    <apex:inputField value="{!obj.BCEOCD__c}"  styleClass="Clineval-11"  style="display:none;"/><!--OCD -->
