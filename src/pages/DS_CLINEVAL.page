                                    <apex:inputField value="{!obj.BCEPTSD__c}"  styleClass="Clineval-13"  style="display:none;"/><!--PTSD -->
                                    <apex:inputField value="{!obj.BCEADJ__c}"  styleClass="Clineval-14"  style="display:none;"/><!--Adjustment Disorder -->
                                    <apex:inputField value="{!obj.BCEATTPR__c}"  styleClass="Clineval-15"  style="display:none;"/><!--Attachment Problems -->
                                    <apex:inputField value="{!obj.BCEDISYMP__c}"  styleClass="Clineval-16"  style="display:none;"/><!--Dissociative Symptoms -->
