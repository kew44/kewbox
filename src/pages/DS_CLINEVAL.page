                                            </tr>                       
                                        </tbody>
                                        <!--end radiobutton table-->
                                    </table>
                                    
                                    <!--value of radiobutton fields seperate from declaration to maintain one radiobutton per column structure -->
                                    <apex:inputField value="{!obj.BCEADHD__c}"  styleClass="Clineval-1"  style="display:none;"/><!--ADHD-->
