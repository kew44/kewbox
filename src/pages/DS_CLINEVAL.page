<apex:page standardController="DS_Functioning_Forms_B__c" extensions="DS_CLINEVAL_Controller">
  
    <!--JQuery -->
    <apex:includeScript value="{!URLFOR($Resource.jQuery, '/js/jquery-1.9.1.js')}"  /><!--allows for JQuery dependencies-->
    <script> $j = jQuery.noConflict(); </script><!--Reserves the '$' token for JQuery only-->
    
    <!-- Bootstrap -->
        <link href="{!URLFOR($Resource.VFbootstrap3, '/css/VFbootstrap.css')}" rel="stylesheet" media="screen"/>
            <apex:includeScript value="{!URLFOR($Resource.VFbootstrap3, '/js/bootstrap.min.js')}"/>
                
                <!--link to the CSS resource in Salesforce internal document-->
                    <apex:stylesheet value="{!$Resource.CustomCss}" />
                        
                        <!--provides framework for "skip-logic" capabilities-->
                            <script>function setFocusOnLoad() {}</script>
    <script> var skipElement = JSON.parse("{!JSENCODE(skipElementToJson)}"); </script>
    <script src="{!URLFOR($Resource.Scripts, '/skipLogic.js')}"></script>
    
    <!--JQuery script for allowing de-selection of radiobuttons--> 
    <script>
    $j(document).ready(function(){
        
        $j('input[type="radio"]').click(function(){
            
            var previousValue = $j(this).attr('title');
            var val = $j(this).val();
            var name = $j(this).attr('name');
            
            if (previousValue == val) {  // uncheck radio button
                $j(this).removeAttr('checked').change();
                $j("input[name='"+name+"']:radio").attr('title', '');
            }
            else {
                $j("input[name='"+name+"']:radio").attr('title', val);
            }            
        });        
    });    
    </script><!--End JQuery deselect radiobutton value script -->
    
    <!-- "Breadcrumb" heading allows user to navigate to the ClientSummary page -->
    <apex:outputPanel rendered="{!if(forceRedirect = '1', false, true)}" layout="none">
        <script>
        $j(document).ready(function() {
            $j('.bPageTitle').append('<div class="ptBreadcrumb">&nbsp;Â«&nbsp;<a href="/apex/ClientSummary?s=0&id={!client.id}">Back to Client Summary</a></div>');
        });
        </script>
    </apex:outputPanel>
    <!--End breadcrumb link--> 
    
    <!--Begin form content-->
    <apex:form id="DS_CLINEVAL" styleClass="form-horizontal">
        <apex:sectionHeader title="Clinical Evaluation" subtitle="{!subtitle}"/><br/>
        
        <!-- pageblock to show ClientInformation header as described in Utility class-->
        <apex:pageBlock title="Client Information">
            <div id="force">
                <apex:outputText value="{!ClientInfo}" escape="false"/>
            </div>
        </apex:pageBlock><!--end client info pageBlock--> 
        
        <!-- primary content pageBlock container-->
        <apex:pageBlock title="Clinical Evaluation" >
            <apex:pageMessages /><!--placeholding structure that allows for system message display-->
            
            <div id="force">
                
                <table class="table DEMO-table">
                    <tbody>
                        <tr>
                            <td colspan="2">
                            
                                <!--instructions -->
                                <div>
                                    <b>
                                        Based on your clinical evaluation, for questions 1-30 please check each problem/symptom/disorder currently displayed by the child. For question 31, please indicate the <i>primary</i> problems/symptom/disorder currently displayed by the child.  Please note that some of the categories have changed to reflect the DSM-5. 
                                    </b>
                                </div>
                                <!--end instructions-->
                                
                                <!--Clinical Evaluation questionaire -->
                                <div>
                                    <table class="table table-bordered table-hover table-condensed Clinevaltable">
                                    
                                        <thead>
                                            <!--information sub-heading-->
                                            <tr>
                                                <th class="text-center">Clinical Problems, Symptoms, and Disorders</th>
                                                <th class="text-center" colspan="3">Child has/exhibits this problem?<br/>(Select all that apply.)</th>
                                            </tr>
                                            <!--end information-->
                                            
                                            <!-- answer indication sub-heading-->
                                            <tr>
                                                <td/>
                                                <td class="text-center" style="width:100px">   No   </td><!--child does not have or exhibit the problem-->
                                                <td class="text-center" style="width:100px">Probable</td><!--the child probably has or exhibits this problem-->
                                                <td class="text-center" style="width:100px">Definite</td><!--the child definitely has or exhibits this problem-->
                                            </tr>
                                            <!--end answers-->
                                        </thead>
                                        
                                        <!--radiobutton indicator table-->
                                        <tbody>
                                            <!--1. ADHD-->
                                            <tr>
                                                <td><b>1.</b>  Attention-Deficit/Hyperactivity Disorder (ADHD) <p>(persistent pattern of inattention and/or hyperactivity-impulsivity that interferes with functioning or development)</p> </td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-1-0" name="Clineval-1" value="0" title="{!obj.BCEADHD__c}"/></td><!--No ADHD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-1-1" name="Clineval-1" value="1" title="{!obj.BCEADHD__c}"/></td><!--Probably ADHD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-1-2" name="Clineval-1" value="2" title="{!obj.BCEADHD__c}"/></td><!--Definite ADHD-->
                                            </tr>
                                            
                                            <!--2. Bipolar-->
                                            <tr>
                                                <td><b>2.</b>  Bipolar Disorder <p>(presence of manic episode(s) and major depressive episode(s)) (e.g., Bipolar I, Bipolar II, or related disorders)</p> </td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-2-0" name="Clineval-2" value="0" title="{!obj.BCEBIPL__c}"/></td><!--No Bipolar-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-2-1" name="Clineval-2" value="1" title="{!obj.BCEBIPL__c}"/></td><!--Probably Bipolar-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-2-2" name="Clineval-2" value="2" title="{!obj.BCEBIPL__c}"/></td><!--Definite Bipolar-->
                                            </tr>
                                            
                                            <!--3. Depressive Symptoms-->
                                            <tr>
                                                <td><b>3.</b>  Depressive Symptoms <p>(e.g., depressed mood, diminished interest or pleasure in activities)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-3-0" name="Clineval-3" value="0" title="{!obj.BCEDPS__c}"/></td><!--No Depressive Symptoms-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-3-1" name="Clineval-3" value="1" title="{!obj.BCEDPS__c}"/></td><!--Probably Depressive Symptoms-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-3-2" name="Clineval-3" value="2" title="{!obj.BCEDPS__c}"/></td><!--Definite Depressive Symptoms-->
                                            </tr>
                                            
                                            <!--4. Depressive Disorder-->
                                            <tr>
                                                <td><b>4.</b>  Depressive Disorder <p>(Disruptive Mood Dysregulation Disorder, Major Depressive Disorder, or related disorders)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-4-0" name="Clineval-4" value="0" title="{!obj.BCEDEP__c}"/></td><!--No Depressive Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-4-1" name="Clineval-4" value="1" title="{!obj.BCEDEP__c}"/></td><!--Probably Depressive Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-4-2" name="Clineval-4" value="2" title="{!obj.BCEDEP__c}"/></td><!--Definite Depressive Disorder-->
                                            </tr>
                                          
                                            <!--5. Traumatic/Complicated Grief-->
                                            <tr>
                                                <td><b>5.</b>  Traumatic/Complicated Grief <p>(response to significant loss; can include sadness, rumination about the loss, insomnia, poor appetite, etc.)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-5-0" name="Clineval-5" value="0" title="{!obj.BCETG__c}"/></td><!--No Traumatic/Complicated Grief-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-5-1" name="Clineval-5" value="1" title="{!obj.BCETG__c}"/></td><!--Probably Traumatic/Complicated Grief-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-5-2" name="Clineval-5" value="2" title="{!obj.BCETG__c}"/></td><!--Definite Traumatic/Complicated Grief-->
                                            </tr>
                                            
                                            <!--6. Generalized Anxiety Disorder-->
                                            <tr>
                                                <td><b>6.</b>  Generalized Anxiety Disorder <p>(excessive anxiety and worry about a number of events or activities, such as work or school performance; difficulty controlling worry)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-6-0" name="Clineval-6" value="0" title="{!obj.BCEGA__c}"/></td><!--No Generalized Anxiety Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-6-1" name="Clineval-6" value="1" title="{!obj.BCEGA__c}"/></td><!--Probably Generalized Anxiety Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-6-2" name="Clineval-6" value="2" title="{!obj.BCEGA__c}"/></td><!--Definite Generalized Anxiety Disorder-->
                                            </tr>
                                            
                                            <!--7. Separation Anxiety-->
                                            <tr>
                                                <td><b>7.</b>  Separation Anxiety Disorder <p>(developmentally inappropriate and excessive fear or anxiety concerning separation from those to whom the individual is attached)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-7-0" name="Clineval-7" value="0" title="{!obj.BCESEPDS__c}"/></td><!--No Separation Anxiety-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-7-1" name="Clineval-7" value="1" title="{!obj.BCESEPDS__c}"/></td><!--Probably Separation Anxiety-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-7-2" name="Clineval-7" value="2" title="{!obj.BCESEPDS__c}"/></td><!--Definite Separation Anxiety-->
                                            </tr>
                                            
                                            <!--8. Social Phobia-->
                                            <tr>
                                                <td><b>8.</b>  Social Anxiety Disorder (Social Phobia) <p>(fear or anxiety about one or more social situations in which the individual is exposed to possible scrutiny by others)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-8-0" name="Clineval-8" value="0" title="{!obj.BCESOCAD__c}"/></td><!--No Social Phobia-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-8-1" name="Clineval-8" value="1" title="{!obj.BCESOCAD__c}"/></td><!--Probably Social Phobia-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-8-2" name="Clineval-8" value="2" title="{!obj.BCESOCAD__c}"/></td><!--Definite Social Phobia-->
                                            </tr>
                                            
                                            <!--9. Panic Disorder-->
                                            <tr>
                                                <td><b>9.</b>  Panic Disorder <p>(recurrent unexpected panic attacks)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-9-0" name="Clineval-9" value="0" title="{!obj.BCEPANDS__c}"/></td><!--No Panic Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-9-1" name="Clineval-9" value="1" title="{!obj.BCEPANDS__c}"/></td><!--Probably Panic Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-9-2" name="Clineval-9" value="2" title="{!obj.BCEPANDS__c}"/></td><!--Definite Panic Disorder-->
                                            </tr>
                                            
                                            <!--10. Specific Phobia-->
                                            <tr>
                                                <td><b>10.</b> Specific Phobia <p>(marked fear or anxiety about a specific object or situation)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-10-0" name="Clineval-10" value="0" title="{!obj.BCEPHODS__c}"/></td><!--No Specific Phobia-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-10-1" name="Clineval-10" value="1" title="{!obj.BCEPHODS__c}"/></td><!--Probably Specific Phobia-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-10-2" name="Clineval-10" value="2" title="{!obj.BCEPHODS__c}"/></td><!--Definite Specific Phobia-->
                                            </tr>
                                            
                                            <!--11. OCD-->
                                            <tr>
                                                <td><b>11.</b> Obsessive Compulsive and Related Disorders <p>(presence of obsessions, compulsions, or both)</p> <p>(e.g., trichotillomania [pulling out own hair], hoarding, body dysmorphic disorder, etc.)</p> </td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-11-0" name="Clineval-11" value="0" title="{!obj.BCEOCD__c}"/></td><!--No OCD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-11-1" name="Clineval-11" value="1" title="{!obj.BCEOCD__c}"/></td><!--Probably OCD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-11-2" name="Clineval-11" value="2" title="{!obj.BCEOCD__c}"/></td><!--Definite OCD-->
                                            </tr>
                                            
                                            <!--12. Acute Stress Disorder-->
                                            <tr>
                                                <td><b>12.</b> Acute Stress Disorder <p> (exposure to actual or threatened death, serious injury, or sexual violation and presence of associated symptoms for up to one month after trauma exposure)</p> </td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-12-0" name="Clineval-12" value="0" title="{!obj.BCEASD__c}"/></td><!--No Acute Stress Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-12-1" name="Clineval-12" value="1" title="{!obj.BCEASD__c}"/></td><!--Probably Acute Stress Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-12-2" name="Clineval-12" value="2" title="{!obj.BCEASD__c}"/></td><!--Definite Acute Stress Disorder-->
                                            </tr>
                                            
                                            <!--13. PTSD-->
                                            <tr>
                                                <td><b>13.</b> Post-Traumatic Stress Disorder <p> (exposure to actual or threatened death, serious injury, or sexual violence and the presence of associated symptoms for more than one month)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-13-0" name="Clineval-13" value="0" title="{!obj.BCEPTSD__c}"/></td><!--No PTSD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-13-1" name="Clineval-13" value="1" title="{!obj.BCEPTSD__c}"/></td><!--Probably PTSD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-13-2" name="Clineval-13" value="2" title="{!obj.BCEPTSD__c}"/></td><!--Definite PTSD-->
                                            </tr>
                                            
                                            <!--14. Adjustment Disorder-->
                                            <tr>
                                                <td><b>14.</b> Adjustment Disorder <p>(development of emotional or behavioral symptoms in response to an identifiable stressor(s) occurring within 3 months of the onset of the stressor(s))</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-14-0" name="Clineval-14" value="0" title="{!obj.BCEADJ__c}"/></td><!--No Adjustment Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-14-1" name="Clineval-14" value="1" title="{!obj.BCEADJ__c}"/></td><!--Probably Adjustment Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-14-2" name="Clineval-14" value="2" title="{!obj.BCEADJ__c}"/></td><!--Definite Adjustment Disorder-->
                                            </tr>
                                            
                                            <!--15. Attachment Problems-->
                                            <tr>
                                                <td><b>15.</b> Attachment Problems/ Difficulties <p>(difficulty forming and maintaining trusting relationships with other people)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-15-0" name="Clineval-15" value="0" title="{!obj.BCEATTPR__c}"/></td><!--No Attachment Problems-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-15-1" name="Clineval-15" value="1" title="{!obj.BCEATTPR__c}"/></td><!--Probably Attachment Problems-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-15-2" name="Clineval-15" value="2" title="{!obj.BCEATTPR__c}"/></td><!--Definite Attachment Problems-->
                                            </tr>
                                            
                                            <!--16. Dissociative Symptoms-->
                                            <tr>
                                                <td><b>16.</b> Dissociative Symptoms <p>(disruption of and/or discontinuity in the normal integration of consciousness, memory, emotion, behavior, etc.)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-16-0" name="Clineval-16" value="0" title="{!obj.BCEDISYMP__c}"/></td><!--No Dissociative Symptoms-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-16-1" name="Clineval-16" value="1" title="{!obj.BCEDISYMP__c}"/></td><!--Probably Dissociative Symptoms-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-16-2" name="Clineval-16" value="2" title="{!obj.BCEDISYMP__c}"/></td><!--Definite Dissociative Symptoms-->
                                            </tr>
                                            
                                            <!--17. Dissociative Disorder-->
                                            <tr>
                                                <td><b>17.</b> Dissociative Disorder <p>(e.g., Dissociative Identity Disorder, Dissociative Amnesia, etc.)</p> </td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-17-0" name="Clineval-17" value="0" title="{!obj.BCEDISSO__c}"/></td><!--No Dissociative Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-17-1" name="Clineval-17" value="1" title="{!obj.BCEDISSO__c}"/></td><!--Probably Dissociative Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-17-2" name="Clineval-17" value="2" title="{!obj.BCEDISSO__c}"/></td><!--Definite Dissociative Disorder-->
                                            </tr>
                                            
                                            <!--18. Somatization -->
                                            <tr>
                                                <td><b>18.</b> Somatization <p>(one or more somatic symptoms that are distressing or result in significant disruption in daily life)</p> <p>(e.g., abdominal pain, headache, fatigue, nausea, etc.)</p> </td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-18-0" name="Clineval-18" value="0" title="{!obj.BCESOMAT__c}"/></td><!--No Somatization -->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-18-1" name="Clineval-18" value="1" title="{!obj.BCESOMAT__c}"/></td><!--Probably Somatization -->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-18-2" name="Clineval-18" value="2" title="{!obj.BCESOMAT__c}"/></td><!--Definite Somatization -->
                                            </tr>
                                            
                                            <!--19. Feeding/Eating Disorder-->
                                            <tr>
                                                <td><b>19.</b> Feeding and Eating Disorder <p>(persistent disturbance of eating or eating-related behavior that results in the altered consumption or absorption of food and that significantly impairs physical health or psychosocial functioning)</p><p>(e.g., Pica, Anorexia Nervosa, Binge-Eating Disorder, etc.)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-19-0" name="Clineval-19" value="0" title="{!obj.BCEEAT__c}"/></td><!--No Feeding/Eating Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-19-1" name="Clineval-19" value="1" title="{!obj.BCEEAT__c}"/></td><!--Probably Feeding/Eating Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-19-2" name="Clineval-19" value="2" title="{!obj.BCEEAT__c}"/></td><!--Definite Feeding/Eating Disorder-->
                                            </tr>
                                            
                                            <!--20. Sleep-Wake Disorder-->
                                            <tr>
                                                <td><b>20.</b>  Sleep-Wake Disorder <p>(sleep-wake complaints regarding the quality, timing, and amount of sleep)</p><p>(e.g., narcolepsy, insomnia, etc.)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-20-0" name="Clineval-20" value="0" title="{!obj.BCESLPDS__c}"/></td><!--No Sleep-Wake Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-20-1" name="Clineval-20" value="1" title="{!obj.BCESLPDS__c}"/></td><!--Probably Sleep-Wake Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-20-2" name="Clineval-20" value="2" title="{!obj.BCESLPDS__c}"/></td><!--Definite Sleep-Wake Disorder-->
                                            </tr>
                                            
                                            <!--21. Sexual Behavioral Problems-->
                                            <tr>
                                                <td><b>21.</b>  Sexual Behavioral Problems <p>(e.g., saying or doing things about sex that is age-inappropriate)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-21-0" name="Clineval-21" value="0" title="{!obj.BCESEXPR__c}"/></td><!--No Sexual Behavioral Problems-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-21-1" name="Clineval-21" value="1" title="{!obj.BCESEXPR__c}"/></td><!--Probably Sexual Behavioral Problems-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-21-2" name="Clineval-21" value="2" title="{!obj.BCESEXPR__c}"/></td><!--Definite Sexual Behavioral Problems-->
                                            </tr>
                                            
                                            <!--22. ODD-->
                                            <tr>
                                                <td><b>22.</b>  Oppositional Defiant Disorder <p>(e.g., pattern of angry/irritable mood, argumentative/defiant behavior, or vindictiveness)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-22-0" name="Clineval-22" value="0" title="{!obj.BCEODD__c}"/></td><!--No ODD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-22-1" name="Clineval-22" value="1" title="{!obj.BCEODD__c}"/></td><!--Probably ODD-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-22-2" name="Clineval-22" value="2" title="{!obj.BCEODD__c}"/></td><!--Definite ODD-->
                                            </tr>
                                            
                                            <!--23. Conduct Disorder-->
                                            <tr>
                                                <td><b>23.</b>  Conduct Disorder <p>(e.g., repetitive and persistent pattern of behavior in which the basic rights of others or major age-appropriate society norms or rules are violated)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-23-0" name="Clineval-23" value="0" title="{!obj.BCECD__c}"/></td><!--No Conduct Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-23-1" name="Clineval-23" value="1" title="{!obj.BCECD__c}"/></td><!--Probably Conduct Disorder-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-23-2" name="Clineval-23" value="2" title="{!obj.BCECD__c}"/></td><!--Definite Conduct Disorder-->
                                            </tr>
                                            
                                            <!--24. IED-->
                                            <tr>
                                                <td><b>24.</b>  Intermittent Explosive Disorder <p>(e.g., recurrent behavioral outbursts representing a failure to control aggressive impulses)</p></td>
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-24-0" name="Clineval-24" value="0" title="{!obj.BCEEXPL__c}"/></td><!--No IED-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-24-1" name="Clineval-24" value="1" title="{!obj.BCEEXPL__c}"/></td><!--Probably IED-->
                                                <td class="text-center"><input type="radio" class="Clineval" id="Clineval-24-2" name="Clineval-24" value="2" title="{!obj.BCEEXPL__c}"/></td><!--Definite IED-->
                                            </tr>
                                            
                                            <!--25. General Behavioral Problems-->
                                            <tr>
